CREATE TABLE [dbo].[dtproperties] (
	[id] int NOT NULL IDENTITY(1,1), 
	[objectid] int, 
	[property] varchar(64) NOT NULL, 
	[value] varchar(255), 
	[uvalue] nvarchar(255), 
	[lvalue] image, 
	[version] int NOT NULL DEFAULT (0),
	CONSTRAINT [pk_dtproperties] PRIMARY KEY ([id], [property])
);


