CREATE TABLE [dbo].[tbl_SubmitNews] (
	[SubmitID] int NOT NULL IDENTITY(1,1) PRIMARY KEY, 
	[SubmitFile] nvarchar(300), 
	[NewsID] int, 
	[SubmitUser] nvarchar(25), 
	[SubmitDate] smalldatetime, 
	[Notes] nvarchar(1000), 
	FOREIGN KEY ([NewsID])
		REFERENCES [dbo].[tbl_News] ([NewsID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);


