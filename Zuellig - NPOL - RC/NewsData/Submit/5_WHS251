CREATE TABLE [dbo].[tblRef_City] (
	[CityID] nvarchar(10) NOT NULL PRIMARY KEY, 
	[CityName] nvarchar(50), 
	[RegionID] nvarchar(25)
);
CREATE TRIGGER [dbo].[tblCity_UTrig] ON tblRef_City 
FOR UPDATE AS
SET NOCOUNT ON
/* * CASCADE UPDATES TO 'tblCurriculumViate' */
IF UPDATE(CityID)
    BEGIN
--/// Permanent Address
       UPDATE tblCurriculumVitae
       SET tblCurriculumVitae.CityID = inserted.CityID
       FROM tblCurriculumVitae, deleted, inserted
       WHERE deleted.CityID = tblCurriculumVitae.CityID


--/// Temporary Address       
       UPDATE tblCurriculumVitae
       SET tblCurriculumVitae.CityID_Temp = inserted.CityID
       FROM tblCurriculumVitae, deleted, inserted
       WHERE deleted.CityID = tblCurriculumVitae.CityID_Temp	

     
    END;
CREATE TRIGGER [dbo].[tblRef_City_DTrig] ON tblRef_City
FOR DELETE 
AS
/* * PREVENT DELETES IF DEPENDENT RECORDS IN 'tblCurriculumVitae' */
IF (SELECT COUNT(*) FROM deleted, tblCurriculumVitae WHERE (deleted.CityID = tblCurriculumVitae.CityID)) > 0
    BEGIN
        RAISERROR 44445 'You can not delete this data because you use it in Curriculum Vitae (Permanent Address) !' --'The record can''t be deleted or changed. Since related records exist in table ''tblCurriculumVitae'', referential integrity rules would be violated.'
        ROLLBACK TRANSACTION
    END

/* * PREVENT DELETES IF DEPENDENT RECORDS IN 'tblCurriculumVitae' */
IF (SELECT COUNT(*) FROM deleted, tblCurriculumVitae WHERE (deleted.CityID = tblCurriculumVitae.CityID_Temp)) > 0
    BEGIN
        RAISERROR 44445 'You can not delete this data because you use it in Curriculum Vitae (Temporary Address) !' --'The record can''t be deleted or changed. Since related records exist in table ''tblCurriculumVitae'', referential integrity rules would be violated.'
        ROLLBACK TRANSACTION
    END

/* * PREVENT DELETES IF DEPENDENT RECORDS IN 'tblCandidate' */
/*IF (SELECT COUNT(*) FROM deleted, tblCandidate WHERE (deleted.CityID = tblCandidate.CityID)) > 0
    BEGIN
        RAISERROR 44445 'You can not delete this data because you use it in Candidate information (Permanent Address) !' --'The record can''t be deleted or changed. Since related records exist in table ''tblCurriculumVitae'', referential integrity rules would be violated.'
        ROLLBACK TRANSACTION
    END
