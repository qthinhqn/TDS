	[CandID] nvarchar(25) NOT NULL, 
	[LangID] nvarchar(10) NOT NULL, 
	[LangLevelID] nvarchar(10), 
	[Speaking] nvarchar(50), 
	[Reading] nvarchar(50), 
	[Writing] nvarchar(50), 
	[Listening] nvarchar(50), 
	[Notes] nvarchar(100),
	CONSTRAINT [PK_tblCandLanguage] PRIMARY KEY ([CandID], [LangID]), 
	FOREIGN KEY ([CandID])
		REFERENCES [dbo].[tblCandidate] ([CandID])
		ON UPDATE CASCADE ON DELETE CASCADE
);


</dx:ASPxPopupControl>;

