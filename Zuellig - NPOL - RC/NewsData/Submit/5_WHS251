CREATE TRIGGER [dbo].[tblRef_Company_Update] On dbo.tblRef_Company
For Update  /* Insert, Update, Delete */
As
	/* If Update (column_name) ...*/

	If Update(CompanyName)
		Update tblGroup
		Set GroupName = inserted.CompanyName
		From Inserted
		Where GroupID = inserted.CompanyID

	If Update(Address)
		Update tblGroup
		Set Notes = inserted.Address
		From Inserted
		Where GroupID = inserted.CompanyID;
CREATE TRIGGER [dbo].[tblRef_Company_UTrig] ON dbo.tblRef_Company
FOR UPDATE AS
SET NOCOUNT ON
/* * CASCADE UPDATES TO 'tblEmployee' */

IF UPDATE(CompanyID)
    BEGIN
--/// tblEmployee
       --// Update CompanyID
       UPDATE tblEmployee
       SET tblEmployee.CompanyID = inserted.CompanyID
       FROM tblEmployee, deleted, inserted
       WHERE deleted.CompanyID = tblEmployee.CompanyID
    END

IF UPDATE(CompanyName)
    BEGIN
--/// tblEmployee
       --// Update CompanyName
       UPDATE tblEmployee
       SET tblEmployee.CompanyName = inserted.CompanyName
       FROM tblEmployee, deleted, inserted
       WHERE deleted.CompanyName = tblEmployee.CompanyName
    END;
CREATE TRIGGER [dbo].[trigInsert_CompID] ON dbo.tblRef_Company
FOR INSERT
AS

BEGIN
	
	INSERT INTO tblGroup (GroupID, GroupName, Notes)
	SELECT CompanyID, CompanyName, Address
	FROM inserted
	WHERE CompanyID NOT IN (Select GroupID From tblGroup Where GroupID = inserted.CompanyID)
END;


CREATE TABLE [dbo].[tblEmpCompany] (
	[EmployeeID] nvarchar(25) NOT NULL, 
	[DateChange] datetime NOT NULL, 
	[CompanyID] nvarchar(50), 
	[Notes] nvarchar(100), 
	[OldCompanyID] nvarchar(10),
	CONSTRAINT [PK_tblEmpCompany] PRIMARY KEY ([EmployeeID], [DateChange]), 
	FOREIGN KEY ([CompanyID])
		REFERENCES [dbo].[tblRef_Company] ([CompanyID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([EmployeeID])
		REFERENCES [dbo].[tblEmployee] ([EmployeeID])
		ON UPDATE CASCADE ON DELETE CASCADE
);
CREATE TRIGGER [dbo].[tblEmpCompany_UTrig] ON dbo.tblEmpCompany FOR UPDATE AS
SET NOCOUNT ON

/* * CASCADE UPDATES TO 'tblEmpChangeCode' */
Declare @intR Int, @intR1 Int, @intR2 Int
Declare @dtmDate DateTime
Declare @EmpID Nvarchar(15)
Declare @EmpCode Nvarchar(9)

SET @intR = 0
SET @intR1 = 0
SET @intR2 = 0

--// inserted: gia tri moi
--// deleted : gia tri cu
--// Ta chia la`m 2 truong hop: 
--	1. Xu ly dvoi du lieu cu
--	2. Xu ly dvoi du lieu moi

IF UPDATE(DateChange)
    BEGIN 	
	--// Dvoi du lieu Cu
	--// Kiem tra xem du lieu trong tblEmpRegion + tblEmpDep co ngay cu hay khong
	--// Neu co' ==> tinh lai gia tri EmpCode cho ngay tuong ung
	--// Neu khong co ==> xoa di du lieu cu
	Select 	@EmpID = EmployeeID From deleted
	Select 	@dtmDate = DateChange From deleted
	
	Select @intR1 = Count(*) 
	From tblEmpRegion
	Where EmployeeID = @EmpID And DateChange = @dtmDate

	Select @intR2 = Count(*) 
	From tblEmpDep
	Where EmployeeID = @EmpID And DateChange = @dtmDate
	
	If @intR1 >0 Or @intR2 > 0 
	   Begin
		exec sp_GetCurrent_EmpCode_WithDate @EmpID, @dtmDate, 0, @EmpCode Output
		
		UPdate tblEmpChangeCode
		Set EmployeeCode  = @EmpCode
		Where EmployeeId = @EmpID And DateChange = @dtmDate
	   End
	Else
		Delete From tblEmpChangeCode
		Where EmployeeId = @EmpID And DateChange = @dtmDate

	--// Doi voi du lieu moi
	--// Xem trong tblEmpChangeCode co hay khong?
	--// Neu co' ==> Update lai gia tri Code
	--// Neu chua co ==> insert gia tri ngay va code moi
	Select 	@EmpID = EmployeeID From inserted
	Select 	@dtmDate = DateChange From inserted
	
	Select	@intR = Count(*)
	From tblEmpChangeCode 
	Where EmployeeID = @EmpID And DateChange = @dtmDate
	
	exec sp_GetCurrent_EmpCode_WithDate @EmpID, @dtmDate, 0, @EmpCode Output
	
	IF  @intR > 0
	   Begin
		UPdate tblEmpChangeCode
		Set EmployeeCode  = @EmpCode
		Where EmployeeId = @EmpID And DateChange = @dtmDate
	   End

       	Else
		Insert Into tblEmpChangeCode (EmployeeID, DateChange, EmployeeCode)
		Values (@EmpID, @dtmDate, @EmpCode)
		--UPDATE tblEmpChangeCode
	       	--SET tblEmpChangeCode.DateChange = inserted.DateChange
       		--FROM tblEmpChangeCode, deleted, inserted
	        --WHERE deleted.DateChange = tblEmpChangeCode.DateChange 
		--	And deleted.EmployeeID = tblEmpChangeCode.EmployeeID
    END

/*
IF UPDATE (CompanyID)
   BEGIN
	Declare @EmpCode Nvarchar(9)
	Declare @EmpID Nvarchar(15)
	Declare @dtmDate DateTime
	
	Select @EmpID = EmployeeID
	From inserted

	Select @dtmDate = DateChange
	From inserted

	exec sp_GetCurrent_EmpCode_WithDate @EmpID,@dtmDate ,1, @EmpCode output
	
	UPDATE tblEmpChangeCode
       	SET tblEmpChangeCode.EmployeeCode = @EmpCode
       	FROM tblEmpChangeCode, deleted
       	WHERE deleted.DateChange = tblEmpChangeCode.DateChange And deleted.EmployeeID = tblEmpChangeCode.EmployeeID

	
	--//==*****************************
	DECLARE @dtmTmp DateTime
	DECLARE Rs CURSOR FOR
		Select DateChange
		From tblEmpChangeCode
		Where DateChange > @dtmDate And EmployeeID = @EmpID
	
	OPEN Rs
	FETCH NEXT FROM Rs INTO @dtmTmp
	WHILE @@Fetch_Status = 0 
	BEGIN
		--// Get gia tri EmployeeCode moi nhat so voi ngay dang nhap moi
		exec sp_GetCurrent_EmpCode_WithDate @EmpID, @dtmTmp, 1, @EmpCode Output
		
		Update tblEmpChangeCode 
		Set EmployeeCode = @EmpCode
		Where EmployeeID = @EmpID And DateChange = @dtmTmp

		FETCH NEXT FROM Rs INTO @dtmTmp
	END
	
	CLOSE Rs
	DEALLOCATE Rs
   END

*/;


