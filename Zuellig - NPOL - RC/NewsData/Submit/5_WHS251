CREATE TABLE [dbo].[tblRef_District] (
	[DistrictID] nvarchar(10) NOT NULL PRIMARY KEY, 
	[DistrictName] nvarchar(50), 
	[CityID] nvarchar(10)
);
CREATE TRIGGER [dbo].[tblDistrict_UTrig] ON tblRef_District FOR UPDATE AS
SET NOCOUNT ON
/* * CASCADE UPDATES TO 'tblCurriculumViate' */
IF UPDATE(DistrictID)
    BEGIN
       UPDATE tblCurriculumVitae
       SET tblCurriculumVitae.DistrictID_Temp = inserted.DistrictID
       FROM tblCurriculumVitae, deleted, inserted
       WHERE deleted.DistrictID = tblCurriculumVitae.DistrictID_Temp

       UPDATE tblCurriculumVitae
       SET tblCurriculumVitae.DistrictID = inserted.DistrictID
       FROM tblCurriculumVitae, deleted, inserted
       WHERE deleted.DistrictID = tblCurriculumVitae.DistrictID
       
    END;
CREATE TRIGGER [dbo].[tblRef_District_DTrig] ON tblRef_District
FOR DELETE 
AS

/* * PREVENT DELETES IF DEPENDENT RECORDS IN 'tblCurriculumVitae' */
IF (SELECT COUNT(*) FROM deleted, tblCurriculumVitae WHERE (deleted.DistrictID = tblCurriculumVitae.DistrictID)) > 0
    BEGIN
        RAISERROR 44445 'You can not delete this data because you use it in Curriculum Vitae (Permanent Address) !' --'The record can''t be deleted or changed. Since related records exist in table ''tblCurriculumVitae'', referential integrity rules would be violated.'
        ROLLBACK TRANSACTION
    END

/* * PREVENT DELETES IF DEPENDENT RECORDS IN 'tblCurriculumVitae' */
IF (SELECT COUNT(*) FROM deleted, tblCurriculumVitae WHERE (deleted.DistrictID = tblCurriculumVitae.DistrictID_Temp)) > 0
    BEGIN
        RAISERROR 44445 'You can not delete this data because you use it in Curriculum Vitae (Temporary Address) !' --'The record can''t be deleted or changed. Since related records exist in table ''tblCurriculumVitae'', referential integrity rules would be violated.'
        ROLLBACK TRANSACTION
    END


/* * PREVENT DELETES IF DEPENDENT RECORDS IN 'tblCandidate' */
/*IF (SELECT COUNT(*) FROM deleted, tblCandidate WHERE (deleted.DistrictID = tblCandidate.DistrictID)) > 0
    BEGIN
        RAISERROR 44445 'You can not delete this data because you use it in Candidate information (Permanent Address) !' --'The record can''t be deleted or changed. Since related records exist in table ''tblCurriculumVitae'', referential integrity rules would be violated.'
        ROLLBACK TRANSACTION
    END
*/

/* * PREVENT DELETES IF DEPENDENT RECORDS IN 'tblCandidate' */
/*
IF (SELECT COUNT(*) FROM deleted, tblCandidate WHERE (deleted.DistrictID = tblCandidate.DistrictID_Temp)) > 0
    BEGIN
        RAISERROR 44445 'You can not delete this data because you use it in Candidate information (Temporary Address) !' --'The record can''t be deleted or changed. Since related records exist in table ''tblCurriculumVitae'', referential integrity rules would be violated.'
        ROLLBACK TRANSACTION
    END
*/;


