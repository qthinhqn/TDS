	[Notes] nvarchar(50), 
	[isDelete] bit
);


CREATE TABLE [dbo].[tblEmpHoliday] (
	[EmployeeID] nvarchar(25) NOT NULL, 
	[DateChange] datetime NOT NULL, 
	[SalaryRate] float, 
	[Notes] nvarchar(255),
	CONSTRAINT [PK_tblEmpHoliday] PRIMARY KEY ([EmployeeID], [DateChange]), 
	FOREIGN KEY ([EmployeeID])
		REFERENCES [dbo].[tblEmployee] ([EmployeeID])
		ON UPDATE CASCADE ON DELETE CASCADE
);


CREATE TABLE [dbo].[tblEmpHoliday_Excel_Tmp] (
	[EmployeeID] nvarchar(25), 
	[DateChange] datetime, 
	[SalaryRate] float, 
	[Notes] nvarchar(255), 
	[IsDelete] bit, 
	[KeyID] int NOT NULL IDENTITY(1,1) PRIMARY KEY
);


CREATE TABLE [dbo].[tblEmpHoliday_Tmp] (
	[EmployeeID] nvarchar(25) NOT NULL, 
	[Dating] datetime NOT NULL
);


CREATE TABLE [dbo].[tblRef_AccidentLevel] (
	[AccLevelID] nvarchar(20) NOT NULL PRIMARY KEY, 
	[AccLevelName] nvarchar(100), 
	[AccLevelName_Foreign] nvarchar(100)
);


CREATE TABLE [dbo].[tblRef_AccidentPersonal] (
	[PersonalID] nvarchar(20) NOT NULL PRIMARY KEY, 
	[PersonalName] nvarchar(100), 
	[PersonalName_Foreign] nvarchar(100)
);


CREATE TABLE [dbo].[tblRef_AccidentTime] (
	[AccTimeID] nvarchar(20) NOT NULL PRIMARY KEY, 
	[AccTimeName] nvarchar(100), 
	[AccTimeName_Foreign] nvarchar(100)
);


CREATE TABLE [dbo].[tblRef_AccidentType] (
	[TypeID] nvarchar(10) NOT NULL PRIMARY KEY, 
	[TypeName] nvarchar(100), 
	[Note] nvarchar(255)
);


CREATE TABLE [dbo].[tblEmpIndusAccident] (
	[EmployeeID] nvarchar(25) NOT NULL, 
	[AccidentDate] datetime NOT NULL, 
	[DateInput] datetime, 
	[FromDate] datetime, 
	[ToDate] datetime, 
	[TypeID] nvarchar(10), 
	[TransportUse] nvarchar(255), 
	[AccLevelID] nvarchar(20), 
	[Place] nvarchar(255), 
	[Detail] nvarchar(500), 
	[Solution] nvarchar(500), 
	[Result] nvarchar(255), 
	[Reason] nvarchar(255), 
	[Injury] nvarchar(255), 
	[Treatment] nvarchar(255), 
	[TreatmentPlace] nvarchar(255), 
	[Documents] nvarchar(500), 
	[MedicalCost] float, 
	[SalaryCost] float, 
	[InsurancePlace] nvarchar(255), 
	[StartDate] datetime, 
	[SubmitDate] datetime, 
	[ProfileStatus] nvarchar(255), 
	[MedicalCost_Approved] float, 
	[SalaryCost_Approved] float, 
	[Amount] float, 
	[SalaryRate] float, 
	[CurrType] nvarchar(20), 
	[Advance] float, 
	[Remain] float, 
	[Birthday] nvarchar(10), 
	[PosName] nvarchar(100), 
	[SectionName] nvarchar(100), 
	[YearWorking] float, 
	[ContractTypeName] nvarchar(100), 
	[BasicSalary] float, 
	[FamilySituation] nvarchar(100), 
	[IsReceive] bit, 
	[PersonalID] nvarchar(20), 
	[AccTimeID] nvarchar(20), 
	[Days] float,
	CONSTRAINT [PK_tblEmpIndusAccident] PRIMARY KEY ([EmployeeID], [AccidentDate]), 
	FOREIGN KEY ([PersonalID])
		REFERENCES [dbo].[tblRef_AccidentPersonal] ([PersonalID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([AccLevelID])
		REFERENCES [dbo].[tblRef_AccidentLevel] ([AccLevelID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([AccTimeID])
		REFERENCES [dbo].[tblRef_AccidentTime] ([AccTimeID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([TypeID])
		REFERENCES [dbo].[tblRef_AccidentType] ([TypeID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([EmployeeID])
		REFERENCES [dbo].[tblEmployee] ([EmployeeID])
		ON UPDATE CASCADE ON DELETE CASCADE
);
