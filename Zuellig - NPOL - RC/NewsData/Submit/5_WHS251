	[EmployeeID] nvarchar(20), 
	[Datechange] datetime, 
	[T_FromTime] int, 
	[T_ToTime] int, 
	[FromTime] datetime, 
	[ToTime] datetime, 
	[ShiftID] nvarchar(30)
);


CREATE TABLE [dbo].[tbl_tinh_ForEarly] (
	[EmployeeID] nvarchar(20), 
	[Ngay_cc] datetime, 
	[Giora_ngay] int, 
	[Giora_Dem] int
);


CREATE TABLE [dbo].[tbl_tinh_ForLate] (
	[EmployeeID] nvarchar(20), 
	[Ngay_cc] datetime, 
	[Giora_ngay] int, 
	[Giora_Dem] int
);


CREATE TABLE [dbo].[tbl_TinhGioGoOut] (
	[EmployeeID] nvarchar(20), 
	[Ngay_CC] datetime, 
	[Tonggio] int, 
	[TotalWD] float, 
	[TotalWN] float, 
	[ShiftID] nvarchar(20)
);


CREATE TABLE [dbo].[tbl_TrainRef_Skill] (
	[SkillID] nvarchar(20) NOT NULL PRIMARY KEY, 
	[SkillName] nvarchar(50)
);
CREATE TRIGGER [dbo].[tbl_TrainRef_Skill_UTrig] ON tbl_TrainRef_Skill FOR UPDATE AS
SET NOCOUNT ON

/* * CASCADE UPDATES TO */
IF UPDATE(SkillID)

       UPDATE tbl_TrainCourse
       SET tbl_TrainCourse.SkillID = inserted.SkillID
       FROM tbl_TrainCourse, deleted, inserted
       WHERE deleted.SkillID = tbl_TrainCourse.SkillID
	
       UPDATE tbl_TrainRequest
       SET tbl_TrainRequest.SkillID = inserted.SkillID
       FROM tbl_TrainRequest, deleted, inserted
       WHERE deleted.SkillID = tbl_TrainRequest.SkillID
	
       UPDATE tbl_TrainPlan
       SET tbl_TrainPlan.SkillID = inserted.SkillID
       FROM tbl_TrainPlan, deleted, inserted
       WHERE deleted.SkillID = tbl_TrainPlan.SkillID
	
       UPDATE tbl_TrainPropose
       SET tbl_TrainPropose.SkillID = inserted.SkillID
       FROM tbl_TrainPropose, deleted, inserted
       WHERE deleted.SkillID = tbl_TrainPropose.SkillID;


CREATE TABLE [dbo].[tbl_TrainBudget_Comp] (
	[KeyID] int NOT NULL PRIMARY KEY, 
	[YearID] int NOT NULL, 
	[FY] nvarchar(10), 
	[Serial] int, 
	[SectionID] nvarchar(10), 
	[LineID] nvarchar(10), 
	[DepID] nvarchar(10), 
	[TeamID] nvarchar(10), 
	[PartID] nvarchar(10), 
	[FieldID] nvarchar(20), 
	[SkillID] nvarchar(20), 
	[Amount] float, 
	[CurrType] nvarchar(20), 
	[Notes] nvarchar(500), 
	[TotalEmp] int, 
	[TrainTypeID] nvarchar(10), 
	FOREIGN KEY ([SkillID])
		REFERENCES [dbo].[tbl_TrainRef_Skill] ([SkillID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Năm tài chính',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainBudget_Comp',@level2type=N'Column',@level2name=N'FY';

CREATE NONCLUSTERED INDEX [IX_tbl_TrainBudget_Comp]
	ON [dbo].[tbl_TrainBudget_Comp] ([YearID], [SectionID], [LineID], [DepID], [TrainTypeID], [FieldID], [SkillID], [TotalEmp]);


CREATE TABLE [dbo].[tbl_TrainBudget_Comp_Excel_Tmp] (
	[YearID] int, 
	[FY] nvarchar(10), 
	[Serial] int, 
	[SectionID] nvarchar(100), 
	[LineID] nvarchar(100), 
	[DepID] nvarchar(100), 
	[TeamID] nvarchar(100), 
	[PartID] nvarchar(100), 
	[FieldID] nvarchar(100), 
	[SkillID] nvarchar(100), 
	[Amount] float, 
	[CurrType] nvarchar(100), 
	[Notes] nvarchar(500), 
	[IsDelete] bit, 
	[TotalEmp] int, 
	[TrainTypeID] nvarchar(10), 
	[KeyID] int NOT NULL IDENTITY(8,1)
);
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Năm tài chính',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainBudget_Comp_Excel_Tmp',@level2type=N'Column',@level2name=N'FY';


