CREATE TABLE [dbo].[tblPayroll_TotalWD_Tmp] (
	[YearID] int NOT NULL, 
	[MonthID] int NOT NULL, 
	[EmployeeID] nvarchar(25) NOT NULL, 
	[EmployeeName] nvarchar(60), 
	[EmpTypeID] nvarchar(10), 
	[EmpTypeName] nvarchar(100), 
	[IsImport] bit, 
	[IsInput] bit, 
	[FromDate] datetime, 
	[ToDate] datetime, 
	[TotalDays] int, 
	[WD] float, 
	[WD_O] float, 
	[WD_N] float, 
	[WH] float, 
	[WH_O] float, 
	[WH_N] float, 
	[OT15] float, 
	[OT15_O] float, 
	[OT15_N] float, 
	[OT15N] float, 
