EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Bo phan duoc phep ky hop dong khong thoi han',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tblRef_Department',@level2type=N'Column',@level2name=N'IsUnlimitContract';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Bo phan duoc huong PC gio giai lao dac biet',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tblRef_Department',@level2type=N'Column',@level2name=N'IsBreakAllowance';

CREATE TRIGGER [dbo].[tblRef_Department_UTrig] ON tblRef_Department 
FOR UPDATE AS
SET NOCOUNT ON

/* * CASCADE UPDATES TO 'tblEmpDep' */
IF UPDATE(DepID)
    BEGIN
--/// tblEmpDep
       --// Update SectionID
       UPDATE tblEmpDep
       SET tblEmpDep.SectionID = inserted.DepID
       FROM tblEmpDep, deleted, inserted
       WHERE deleted.DepID = tblEmpDep.SectionID

       --// Update LineID
       UPDATE tblEmpDep
       SET tblEmpDep.LineID = inserted.DepID
       FROM tblEmpDep, deleted, inserted
       WHERE deleted.DepID = tblEmpDep.LineID

       --// Update DepID
       UPDATE tblEmpDep
       SET tblEmpDep.DepID = inserted.DepID
       FROM tblEmpDep, deleted, inserted
       WHERE deleted.DepID = tblEmpDep.DepID

--// Update DepID
       UPDATE tblEmpDep
       SET tblEmpDep.TeamID = inserted.DepID
       FROM tblEmpDep, deleted, inserted
       WHERE deleted.DepID = tblEmpDep.TeamID

--/// tblEmployee
       --// Update SectionID
       UPDATE tblEmployee
       SET tblEmployee.SectionID = inserted.DepID
       FROM tblEmployee, deleted, inserted
       WHERE deleted.DepID = tblEmployee.SectionID
 
       --// Update LineID
       UPDATE tblEmployee
       SET tblEmployee.LineID = inserted.DepID
       FROM tblEmployee, deleted, inserted
       WHERE deleted.DepID = tblEmployee.LineID
       
       --// Update DepID
       UPDATE tblEmployee
       SET tblEmployee.DepID = inserted.DepID
       FROM tblEmployee, deleted, inserted
       WHERE deleted.DepID = tblEmployee.DepID
       
       --// Update TeamID
       UPDATE tblEmployee
       SET tblEmployee.TeamID = inserted.DepID
       FROM tblEmployee, deleted, inserted
       WHERE deleted.DepID = tblEmployee.TeamID
	
  --// Update PartID
       UPDATE tblEmployee
       SET tblEmployee.PartID = inserted.DepID
       FROM tblEmployee, deleted, inserted
       WHERE deleted.DepID = tblEmployee.PartID

--/// tblEmpLunch	     
       UPDATE tblEmpLunch
       SET tblEmpLunch.PartID = inserted.DepID
       FROM tblEmpLunch, deleted, inserted
       WHERE deleted.DepID = tblEmpLunch.PartID
 	
--/// tblRef_Department --- Update lai DepParent
       UPDATE tblRef_Department
       SET tblRef_Department.DepParent = inserted.DepID
       FROM tblRef_Department, deleted, inserted
       WHERE deleted.DepID = tblRef_Department.DepParent
   END

IF UPDATE (DepName)
   BEGIN
--/// tblEmployee
       --// Update SectionName
       UPDATE tblEmployee
       SET tblEmployee.SectionName = inserted.DepName
       FROM tblEmployee, deleted, inserted
       WHERE deleted.DepName = tblEmployee.SectionName

       --/// Update LineName
       UPDATE tblEmployee
       SET tblEmployee.LineName = inserted.DepName
       FROM tblEmployee, deleted, inserted
       WHERE deleted.DepName = tblEmployee.LineName

       --/// Update DepName
       UPDATE tblEmployee
       SET tblEmployee.DepName = inserted.DepName
       FROM tblEmployee, deleted, inserted
       WHERE deleted.DepName = tblEmployee.DepName

       --/// Update TeamName
       UPDATE tblEmployee
       SET tblEmployee.TeamName = inserted.DepName
       FROM tblEmployee, deleted, inserted
       WHERE deleted.DepName = tblEmployee.TeamName
   END;


CREATE TABLE [dbo].[tblShift] (
	[ShiftID] nvarchar(10) NOT NULL PRIMARY KEY, 
	[ShiftName] nvarchar(50), 
	[FromTime] datetime, 
	[ToTime] datetime, 
	[Begin1] datetime, 
	[End1] datetime, 
	[DeductBreak1] bit, 
	[BeginMeal] datetime, 
	[EndMeal] datetime, 
	[DeductOT_SunPH] bit, 
	[Begin2] datetime, 
	[End2] datetime, 
	[DeductBreak2] bit, 
	[BeginMealOT] datetime, 
	[EndMealOT] datetime, 
	[DeductMealOT] bit, 
	[BeginOT_Before] datetime, 
	[EndOT_Before] datetime, 
	[BeginOT_After] datetime, 
	[EndOT_After] datetime, 
	[NightShift] bit, 
	[T_FromTime] smallint, 
	[T_ToTime] smallint, 
	[T_Begin1] smallint, 
	[T_End1] smallint, 
	[T_BeginMeal] smallint, 
	[T_EndMeal] smallint, 
	[T_Begin2] smallint, 
	[T_End2] smallint, 
	[T_BeginOT_Before] smallint, 
	[T_EndOT_Before] smallint, 
	[T_BeginOT_After] smallint, 
	[T_EndOT_After] smallint, 
	[T_BeginMealOT] smallint, 
	[T_EndMealOT] smallint, 
	[Weekdays] int, 
	[ShiftDep] nvarchar(10), 
	[T_M_Meal] smallint, 
	[M_Meal] datetime
);


CREATE TABLE [dbo].[tblDepShift] (
	[DateChange] datetime NOT NULL, 
	[ShiftID] nvarchar(10) NOT NULL, 
	[SectionID] nvarchar(10), 
	[LineID] nvarchar(10) NOT NULL, 
	[DepID] nvarchar(10), 
	[TeamID] nvarchar(10), 
	[PartID] nvarchar(10), 
	[Notes] nvarchar(50),
	CONSTRAINT [PK_tblDepShift] PRIMARY KEY ([DateChange], [ShiftID], [LineID]), 
	FOREIGN KEY ([LineID])
		REFERENCES [dbo].[tblRef_Department] ([DepID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([ShiftID])
		REFERENCES [dbo].[tblShift] ([ShiftID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);


