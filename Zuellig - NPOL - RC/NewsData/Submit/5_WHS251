CREATE TABLE [dbo].[tblCand_Interview_Emp] (
	[Num] int NOT NULL, 
	[InterviewDate] datetime NOT NULL, 
	[Interviewer] nvarchar(100) NOT NULL, 
	[EmployeeID] nvarchar(25), 
	[Notes] nvarchar(255), 
	[TotalFee] float, 
	[CurrType] nvarchar(50), 
	[PosName] nvarchar(100),
	CONSTRAINT [PK_tblCand_Interview_Emp] PRIMARY KEY ([Num], [InterviewDate], [Interviewer]), 
	FOREIGN KEY ([InterviewDate], [Num])
		REFERENCES [dbo].[tblCand_Interview] ([InterviewDate], [Num])
		ON UPDATE CASCADE ON DELETE CASCADE
);


CREATE TABLE [dbo].[tblCand_Interview_Request] (
	[InterviewDate] datetime NOT NULL, 
	[Num] int NOT NULL, 
	[RequestID] nvarchar(50) NOT NULL, 
	[Notes] nvarchar(255), 
	[PosID] nvarchar(10),
	CONSTRAINT [PK_tblCand_Interview_Request] PRIMARY KEY ([InterviewDate], [Num], [RequestID]), 
	FOREIGN KEY ([InterviewDate], [Num])
		REFERENCES [dbo].[tblCand_Interview] ([InterviewDate], [Num])
		ON UPDATE CASCADE ON DELETE CASCADE, 
	FOREIGN KEY ([RequestID])
		REFERENCES [dbo].[tblCand_Request] ([RequestID])
		ON UPDATE CASCADE ON DELETE CASCADE
);


