	[AdTypeID] nvarchar(10), 
	[TotalCand_Supplier] int, 
	[TotalCand] int, 
	[Cand_Rate] float, 
	[Recruited_Rate] float, 
	FOREIGN KEY ([AdTypeID])
		REFERENCES [dbo].[tblCandRef_AdvertiseType] ([AdTypeID])
		ON UPDATE CASCADE ON DELETE CASCADE, 
	FOREIGN KEY ([SupplierID])
		REFERENCES [dbo].[tblCand_Supplier] ([SupplierID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);


