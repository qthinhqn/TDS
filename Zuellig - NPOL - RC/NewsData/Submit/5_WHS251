	[Birthday] nvarchar(10), 
	[Address] nvarchar(255), 
	[IDCard] nvarchar(50), 
	[PosID] nvarchar(25), 
	[RecruitByID] nvarchar(25), 
	[LocationID] nvarchar(50), 
	[SectionID] nvarchar(50), 
	[LineID] nvarchar(50), 
	[DepID] nvarchar(50), 
	[LevelID] nvarchar(50), 
	[ReportTo] nvarchar(255), 
	[TempDays] int, 
	[StartDate] datetime, 
	[Transport] float, 
	[Allowance] float, 
	[Probation_Sal] float, 
	[Permanent_Sal] float, 
	[ReturnDate] datetime, 
	[CompleteDate] datetime, 
	[MakingDate] datetime, 
	[SendingDate] datetime, 
	[ResendDate] datetime, 
	[WhomReceived] nvarchar(255), 
	[ReceivedDate] datetime, 
	[BookCode] bit, 
	[Degree] nvarchar(255), 
	[Notes] nvarchar(2000), 
	[IsDelete] bit, 
	[KeyID] int NOT NULL IDENTITY(1,1) PRIMARY KEY, 
	[Parking] float, 
	[Internet] float, 
	[Meal] float, 
	[Mobile] float, 
	[SexID] nvarchar(10), 
	[EmployeeName] nvarchar(150), 
	[PosName] nvarchar(255), 
	[PosName_VN] nvarchar(255), 
	[WorkingLocaion] nvarchar(255), 
	[ControlLocation] nvarchar(255), 
	[IsNewHeadCount] bit, 
	[EmpID_Replace] nvarchar(25), 
	[EmpName_Replace] nvarchar(150), 
	[ReportTo_VN] nvarchar(255)
);
