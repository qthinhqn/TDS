CREATE TABLE [dbo].[tblSalaryLevel] (
	[SalLevelID] nvarchar(15) NOT NULL PRIMARY KEY, 
	[SalLevelName] nvarchar(50), 
	[Amount] float, 
	[TitleID] nvarchar(10), 
	[Coefficient] float, 
	[DateChange] datetime, 
	[Notes] nvarchar(50), 
	FOREIGN KEY ([TitleID])
		REFERENCES [dbo].[tblRef_Title] ([TitleID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);


CREATE TABLE [dbo].[tblEmpSalary] (
	[EmployeeID] nvarchar(25) NOT NULL, 
	[DateChange] datetime NOT NULL, 
	[TitleID] nvarchar(10), 
	[SalLevelID] nvarchar(15), 
	[Coefficient] float, 
	[BasicSalary] nvarchar(200), 
	[CurrType] nvarchar(20), 
	[Notes] nvarchar(100), 
	[BasicSalary_Ins] float, 
	[EducationID] nvarchar(10), 
	[IsAdd_BySystem] bit, 
	[IsMonthSal] bit, 
	[DateProcess] datetime,
	CONSTRAINT [PK_tblEmpSalary] PRIMARY KEY ([EmployeeID], [DateChange]), 
	FOREIGN KEY ([EmployeeID])
		REFERENCES [dbo].[tblEmployee] ([EmployeeID])
		ON UPDATE CASCADE ON DELETE CASCADE, 
	FOREIGN KEY ([SalLevelID])
		REFERENCES [dbo].[tblSalaryLevel] ([SalLevelID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'= 1: hệ thống tự động tính dựa vào bảng lương tháng; = 0 : người sd thêm ',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tblEmpSalary',@level2type=N'Column',@level2name=N'IsAdd_BySystem';
