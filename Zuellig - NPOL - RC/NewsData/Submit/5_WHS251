CREATE TABLE [dbo].[tblEmpDiscipline] (
	[EmployeeID] nvarchar(25) NOT NULL, 
	[DateChange] datetime NOT NULL, 
	[NumOf] tinyint NOT NULL DEFAULT (1), 
	[Behaviour] nvarchar(50), 
	[DisciplineID] nvarchar(10), 
	[DecisionNo] nvarchar(20), 
	[FromDate] datetime, 
	[ToDate] datetime, 
	[Content] nvarchar(255), 
	[PreviousBehaviour] nvarchar(255), 
	[SuggestRespon] nvarchar(255), 
	[DeadlineReform] nvarchar(255), 
	[IsSuspension] bit, 
	[Notes] nvarchar(255), 
	[TypeID] nvarchar(10), 
	[DecisionDate] datetime, 
	[From_MY] nvarchar(7), 
	[To_MY] nvarchar(7), 
	[Amount] float, 
	[CurrType] nvarchar(20), 
	[IsDeductDiligence] bit, 
	[IsMonthlyDeduct] bit,
	CONSTRAINT [PK_tblEmpDiscipline] PRIMARY KEY ([EmployeeID], [DateChange], [NumOf]), 
	FOREIGN KEY ([EmployeeID])
		REFERENCES [dbo].[tblEmployee] ([EmployeeID])
		ON UPDATE CASCADE ON DELETE CASCADE, 
	FOREIGN KEY ([DisciplineID])
		REFERENCES [dbo].[tblRef_Discipline] ([DisciplineID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);
