CREATE TABLE [dbo].[tblCand_ContractDetail] (
	[TicketID] nvarchar(20) NOT NULL, 
	[RequestID] nvarchar(50) NOT NULL, 
	[Series] nvarchar(50), 
	[SectionID] nvarchar(10), 
	[LineID] nvarchar(10), 
	[DepID] nvarchar(10), 
	[PosID] nvarchar(10), 
	[RegionID] nvarchar(25), 
	[NumOf] int, 
	[UnitPrice] float, 
	[Amount] float, 
	[Result] nvarchar(255), 
	[TotalCand_Supplier] int, 
	[TotalCand] int, 
	[Cand_Rate] float, 
	[Recruited_Rate] float, 
	[TypeID] nvarchar(10), 
	[FromDate] datetime, 
	[ToDate] datetime, 
	[Views] float, 
	[SupplierJobID] nvarchar(50), 
	[Description] nvarchar(500), 
	[RequestBy] nvarchar(150), 
	[KeyID] numeric(19, 0) NOT NULL IDENTITY(1,1) PRIMARY KEY, 
	FOREIGN KEY ([TicketID])
		REFERENCES [dbo].[tblCand_Contract] ([TicketID])
		ON UPDATE CASCADE ON DELETE CASCADE, 
	FOREIGN KEY ([RequestID])
		REFERENCES [dbo].[tblCand_Request] ([RequestID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);


CREATE TABLE [dbo].[tblCandRef_Fee] (
	[FeeID] nvarchar(20) NOT NULL PRIMARY KEY, 
	[FeeName] nvarchar(100)
);


CREATE TABLE [dbo].[tblCand_Fee] (
	[KeyID] int NOT NULL PRIMARY KEY, 
