CREATE TABLE [dbo].[tblCand_ReferenceCheck] (
	[CandID] nvarchar(25) NOT NULL, 
	[CandName] nvarchar(120), 
	[DateChange] datetime NOT NULL, 
	[ContactName] nvarchar(100) NOT NULL, 
	[CheckedBy] nvarchar(100), 
	[WorkAt] nvarchar(255), 
	[PosName] nvarchar(100), 
	[Email] nvarchar(100), 
	[Phone] nvarchar(50), 
	[Comment] nvarchar(255), 
	[Notes] nvarchar(255), 
	[CheckedByPos] nvarchar(255), 
	[CheckedByDept] nvarchar(255), 
	[Relationship] nvarchar(255), 
	[Relationship_Other] nvarchar(255), 
	[ExpectedPosID] nvarchar(10),
	CONSTRAINT [PK_tblCand_ReferenceCheck] PRIMARY KEY ([CandID], [DateChange], [ContactName])
);


CREATE TABLE [dbo].[tblCandRef_ReasonReference] (
	[ReasonID] nvarchar(10) NOT NULL PRIMARY KEY, 
	[ReasonName] nvarchar(255)
);


CREATE TABLE [dbo].[tblCand_ReferenceCheck_Detail] (
	[CandID] nvarchar(25) NOT NULL, 
	[DateChange] datetime NOT NULL, 
	[ContactName] nvarchar(100) NOT NULL, 
	[ReasonID] nvarchar(10) NOT NULL, 
	[Comment] nvarchar(500), 
	[Notes] nvarchar(255),
	CONSTRAINT [PK_tblCand_ReferenceCheck_Detail] PRIMARY KEY ([CandID], [DateChange], [ContactName], [ReasonID]), 
	FOREIGN KEY ([CandID], [ContactName], [DateChange])
		REFERENCES [dbo].[tblCand_ReferenceCheck] ([CandID], [ContactName], [DateChange])
		ON UPDATE CASCADE ON DELETE CASCADE, 
	FOREIGN KEY ([ReasonID])
		REFERENCES [dbo].[tblCandRef_ReasonReference] ([ReasonID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);


CREATE TABLE [dbo].[tblCand_ReferenceCheck_Excel_Tmp] (
	[CandID] nvarchar(25), 
	[DateChange] datetime, 
	[CheckedBy] nvarchar(100), 
	[ContactName] nvarchar(100), 
	[WorkAt] nvarchar(255), 
	[PosName] nvarchar(100), 
	[Email] nvarchar(100), 
	[Phone] nvarchar(50), 
	[ReasonID] nvarchar(10), 
	[Comment] nvarchar(500), 
	[Notes] nvarchar(500), 
	[IsDelete] bit, 
	[KeyID] int NOT NULL IDENTITY(1,1) PRIMARY KEY, 
	[CheckedByPos] nvarchar(255), 
	[CheckedByDept] nvarchar(255), 
	[Relationship] nvarchar(255), 
	[Relationship_Other] nvarchar(255), 
	[ExpectedPosID] nvarchar(10)
);


