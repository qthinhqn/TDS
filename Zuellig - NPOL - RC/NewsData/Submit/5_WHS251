	[KindID] nvarchar(10) NOT NULL PRIMARY KEY, 
	[KindName] nvarchar(100)
);


CREATE TABLE [dbo].[tblGroup] (
	[GroupID] nvarchar(50) NOT NULL PRIMARY KEY, 
	[KindID] nvarchar(10) NOT NULL DEFAULT ('Com'), 
	[GroupName] nvarchar(100), 
	[Notes] nvarchar(255), 
	FOREIGN KEY ([KindID])
		REFERENCES [dbo].[tblKindGroup] ([KindID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([GroupID])
		REFERENCES [dbo].[tblRef_Company] ([CompanyID])
		ON UPDATE CASCADE ON DELETE CASCADE
);


