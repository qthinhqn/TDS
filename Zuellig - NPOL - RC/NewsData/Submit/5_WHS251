CREATE TRIGGER [dbo].[trigInsert_EmpID] ON dbo.tblEmployee
FOR INSERT
AS--Begin

BEGIN
Declare @E Float
	Set @E = 0
	
	--// Tu dong insert vao table tblEmpCompany
	--IF Isnull( (Select inserted.EmployedDate From inserted), '1/1/1990') <> '1/1/1990'
	--   Begin
	 Begin
		INSERT INTO tblEmpCompany (EmployeeId,CompanyID, DateChange)
		SELECT distinct EmployeeID,  (Select distinct CompanyID From tblRef_Company), inserted.EmployedDate
		FROM inserted
		WHERE EmployeeID NOT IN (Select EmployeeID From tblEmpCompany Where EmployeeID = inserted.EmployeeID And DateChange = inserted.EmployedDate)
			And  Isnull(inserted.EmployedDate ,'') <> ''

		Update tblEmployee
		Set 	CompanyID = (Select distinct CompanyID From tblRef_Company)--,
			--CardID =  inserted.EmployeeID
		From tblEmployee Inner Join inserted On tblEmployee.EmployeeID = inserted.EmployeeID


		INSERT INTO tblCurriculumVitae (EmployeeId, Birthday, MaritalStatusID, SexID,  QualificationID, EducationID,
						IDCard, IssuePlace, IssueDate, Address, Phone, Mobile, CompanyID
						)
		SELECT 	EmployeeID, Birthday, MaritalStatusID, SexID,  QualificationID, EducationID,
			IDCard, IssuePlace, IssueDate, Address, Phone, Mobile,
			(Select distinct Top 1 CompanyID From tblRef_Company)
		FROM inserted
		WHERE EmployeeID NOT IN (Select EmployeeID From tblCurriculumVitae Where EmployeeID = inserted.EmployeeID)

	   End

	--   End

	--// Tu dong insert vao table tblEmpChangeCode
	--INSERT INTO tblEmpChangeCode (EmployeeId, EmployeeCode, DateChange)
	--SELECT EmployeeID,  inserted.EmployeeID,  inserted.EmployedDate
	--FROM inserted
	--WHERE EmployeeID NOT IN (Select EmployeeID From tblEmpChangeCode Where EmployeeID = inserted.EmployeeID And DateChange = inserted.EmployedDate)

END;


CREATE TABLE [dbo].[tbl_EmpHandover] (
	[EmployeeID] nvarchar(25), 
	[Datechange] datetime, 
	[RecipientID] nvarchar(25), 
	[Authorizer] nvarchar(150), 
	[Method] nvarchar(500), 
	[SINo] nvarchar(500), 
	[InvoiceNo] nvarchar(500), 
	[TerminationNo] nvarchar(500), 
	[Notes] nvarchar(500), 
	[KeyID] int NOT NULL IDENTITY(1,1) PRIMARY KEY, 
	FOREIGN KEY ([EmployeeID])
		REFERENCES [dbo].[tblEmployee] ([EmployeeID])
		ON UPDATE CASCADE ON DELETE CASCADE
);


CREATE TABLE [dbo].[tbl_EmpLate_Tmp] (
	[EmployeeID] nvarchar(15), 
	[EmployeeName] nvarchar(60), 
	[Ngay_CC] datetime, 
	[ShiftID] nvarchar(10), 
	[FromTime] datetime, 
	[SweepIn] datetime, 
	[T_FromTime] smallint, 
	[T_SweepIn] int, 
	[TotalMinute] int
);


CREATE TABLE [dbo].[tbl_Employee_WD_WN] (
	[KeyID] int NOT NULL IDENTITY(1,1), 
	[EmployeeID] char, 
	[Ngay_CC] datetime, 
	[WD] float, 
	[WN] float, 
	[Total] int, 
	[Late] bit, 
	[Early] bit, 
	[GoOut] bit
);


CREATE TABLE [dbo].[tbl_EmpOT_Attendance] (
	[EmployeeID] nvarchar(15) NOT NULL, 
	[DateChange] datetime NOT NULL, 
	[OT15] float, 
	[OT15Night] float, 
	[OT20] float, 
	[OT20Night] float, 
	[OT30] float, 
	[OT30Night] float, 
	[OT15_Before] numeric(38, 1), 
	[OT15_After] numeric(38, 1), 
	[OT15Night_Before] numeric(38, 1), 
	[OT15Night_After] numeric(38, 1)
);


CREATE TABLE [dbo].[tbl_EmpOT_DangTest] (
	[EmployeeID] nvarchar(15) NOT NULL, 
	[DateChange] datetime NOT NULL, 
	[FromTime] datetime NOT NULL, 
	[DayTotalHours] float, 
	[NightTotalHours] float, 
	[OT_Night] float, 
	[OT_Day] float, 
	[Sunday] bit NOT NULL, 
	[PH] bit NOT NULL, 
	[OTDay_Before] numeric(11, 1), 
	[OTDay_After] numeric(11, 1), 
	[OTNight_Before] numeric(1, 1) NOT NULL, 
	[OTNight_After] numeric(1, 1) NOT NULL, 
	[OTD_Before] numeric(1, 1) NOT NULL, 
	[OTD_After] numeric(1, 1) NOT NULL, 
	[OTN_Before] numeric(1, 1) NOT NULL, 
	[OTN_After] numeric(1, 1) NOT NULL
);


