CREATE TABLE [dbo].[tblPayroll_TmpPrint_Money] (
	[EmployeeID] nvarchar(15) NOT NULL, 
	[EmployeeName] nvarchar(60), 
	[FamilyName] nvarchar(40), 
	[GivenName] nvarchar(20), 
	[CardID] nvarchar(14), 
	[EmpTypeID] nvarchar(2), 
	[EmpTypeName] nvarchar(100), 
	[EmployedDate] datetime, 
	[LeftDate] datetime, 
	[ContractDate] datetime, 
	[SectionID] nvarchar(10), 
	[SectionName] nvarchar(50), 
	[SectionNum] int, 
	[LineID] nvarchar(10), 
	[LineName] nvarchar(100), 
	[LineNum] int, 
	[DepID] nvarchar(10), 
	[DepName] nvarchar(100), 
	[DepNum] int, 
	[TeamID] nvarchar(10), 
	[TeamName] nvarchar(100), 
	[TeamNum] int, 
	[PartID] nvarchar(10), 
	[PartName] nvarchar(100), 
	[PartNum] int, 
	[PosID] nvarchar(10), 
	[PosName] nvarchar(50), 
	[PosNum] int, 
	[TakeHome] float, 
	[M_500] int, 
	[M_200] int, 
	[M_100] int, 
	[M_50] int, 
	[M_20] int, 
	[M_10] int, 
	[M_5] int, 
	[M_2] int, 
	[M_1] int
);
CREATE NONCLUSTERED INDEX [IX_tblPayroll_TmpPrint_Money]
