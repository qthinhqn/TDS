EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Bằng cấp cao nhất',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tblEmpEducation',@level2type=N'Column',@level2name=N'DegreeID';
