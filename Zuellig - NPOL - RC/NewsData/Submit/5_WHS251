	[YearID] int NOT NULL, 
	[Serial] int, 
	[FY] nvarchar(100), 
	[FieldID] nvarchar(100), 
	[SkillID] nvarchar(100), 
	[Amount] float, 
	[CurrType] nvarchar(100), 
	[NumEmp] int, 
	[Notes] nvarchar(500), 
	[IsDelete] bit, 
	[TrainTypeID] nvarchar(10), 
	[KeyID] int NOT NULL IDENTITY(11,1)
);
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Năm tài chính',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainBudget_HR_Excel_Tmp',@level2type=N'Column',@level2name=N'FY';


