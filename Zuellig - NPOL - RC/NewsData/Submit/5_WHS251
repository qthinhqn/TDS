CREATE TABLE [dbo].[tbl_Viewed] (
	[ID] int NOT NULL IDENTITY(1,1) PRIMARY KEY, 
	[UserID] nvarchar(25), 
	[ViewDate] smalldatetime, 
	[NewsID] int, 
	FOREIGN KEY ([NewsID])
		REFERENCES [dbo].[tbl_News] ([NewsID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);
-- Dem so luot xem
CREATE TRIGGER [dbo].[CountUserView] ON tbl_Viewed
FOR INSERT
AS
	DECLARE @NewsID smallint
	
    BEGIN
	    SET @NewsID=(SELECT NewsID FROM INSERTED)
		UPDATE tbl_News 
		SET ViewedCount = ViewedCount + 1
		WHERE NewsID = @NewsID
	END;


