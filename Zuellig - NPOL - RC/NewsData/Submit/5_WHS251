CREATE TABLE [dbo].[tblCand_Inform] (
	[TicketID] nvarchar(20) NOT NULL PRIMARY KEY, 
	[DateChange] datetime, 
	[DeadLine] datetime, 
	[PreparedBy] nvarchar(100), 
	[StatusID] nvarchar(10), 
	[Content] nvarchar(500), 
	[Method] nvarchar(255), 
	[Notes] nvarchar(255), 
	FOREIGN KEY ([StatusID])
		REFERENCES [dbo].[tblCandRef_InformStatus] ([StatusID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);


CREATE TABLE [dbo].[tblCand_InformDetail] (
	[TicketID] nvarchar(20) NOT NULL, 
	[RequestID] nvarchar(50) NOT NULL, 
	[SectionID] nvarchar(10), 
	[LineID] nvarchar(10), 
	[GroupID] nvarchar(10), 
	[PosID] nvarchar(10), 
	[NumOf] int, 
	[TypeID] nvarchar(10),
	CONSTRAINT [PK_tblCand_AnnouncementDetail] PRIMARY KEY ([TicketID], [RequestID]), 
	FOREIGN KEY ([TicketID])
		REFERENCES [dbo].[tblCand_Inform] ([TicketID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([RequestID])
		REFERENCES [dbo].[tblCand_Request] ([RequestID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);


