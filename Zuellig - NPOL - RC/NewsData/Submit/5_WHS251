	[FromDate] datetime, 
	[ToDate] datetime, 
	[Reason] nvarchar(500), 
	[Achieve] nvarchar(500), 
	[ContactName] nvarchar(100), 
	[EmailTo] nvarchar(255), 
	[Notes] nvarchar(255), 
	[IsFromPDR] bit, 
	[IsDelete] bit, 
	[KeyID] int NOT NULL IDENTITY(1,1)
);
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Không sử dụng(Lĩnh vực đào tạo)',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainPlan_Excel_Tmp',@level2type=N'Column',@level2name=N'FieldID';
