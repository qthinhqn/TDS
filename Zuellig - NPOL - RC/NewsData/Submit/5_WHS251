CREATE TABLE [dbo].[tblEmpContract_Liability] (
	[OrderNo] nvarchar(10), 
	[EmployeeID] nvarchar(25) NOT NULL, 
	[DateChange] datetime NOT NULL, 
	[CandID] nvarchar(25), 
	[ContractID] nvarchar(50), 
	[OfferID] nvarchar(50), 
	[TempDate] datetime, 
	[Place_Issue] nvarchar(255), 
	[Place_Issue_E] nvarchar(255), 
	[Guarantor] nvarchar(100), 
	[Place] nvarchar(255), 
	[Place_E] nvarchar(255), 
	[Deposit] float, 
	[IsSend] bit, 
	[IsReturn] bit, 
	[IsReturnEmp] bit, 
	[ReturnEmp] datetime, 
	[IsJudicial] bit, 
	[IsLandDoc] bit, 
	[IsGuarantee] bit, 
	[Notes] nvarchar(500), 
	[SexID] nvarchar(10), 
	[Birthday] datetime, 
	[PlaceOfBirth] nvarchar(100), 
	[PosName] nvarchar(255), 
	[PosName_VN] nvarchar(255), 
	[Address] nvarchar(255), 
	[IDCard] nvarchar(50), 
	[IssuedDate] datetime, 
	[IssuedPlace] nvarchar(150), 
	[WhomReceived] nvarchar(150), 
	[StartDate] datetime,
	CONSTRAINT [PK_tblEmpContract_Liability] PRIMARY KEY ([EmployeeID], [DateChange]), 
	FOREIGN KEY ([EmployeeID])
		REFERENCES [dbo].[tblEmployee] ([EmployeeID])
		ON UPDATE CASCADE ON DELETE CASCADE
);
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Gởi HĐTN cho nhân viên',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tblEmpContract_Liability',@level2type=N'Column',@level2name=N'IsSend';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Trả HD về',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tblEmpContract_Liability',@level2type=N'Column',@level2name=N'IsReturn';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Trả HĐ trách nhiệm cho nhân viên',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tblEmpContract_Liability',@level2type=N'Column',@level2name=N'IsReturnEmp';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Lý lịch tư pháp?',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tblEmpContract_Liability',@level2type=N'Column',@level2name=N'IsJudicial';
