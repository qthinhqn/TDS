CREATE TABLE [dbo].[tblCand_Plan] (
	[KeyID] int NOT NULL PRIMARY KEY, 
	[Priority] int, 
	[RequestID] nvarchar(50), 
	[RecruitByID] nvarchar(25), 
	[SectionID] nvarchar(25), 
	[LineID] nvarchar(25), 
	[DepID] nvarchar(25), 
	[LevelID] nvarchar(25), 
	[PosID] nvarchar(25), 
	[PosName] nvarchar(500), 
	[IsInternalPost] bit, 
	[InPosted_Ad] datetime, 
	[ExPosted_Ad] datetime, 
	[BuySearch_Ad] datetime, 
	[DeadLine_Ad] datetime, 
	[Where_Ad] nvarchar(500), 
	[ShortList_Date] datetime, 
	[InvitedDate] datetime, 
	[AskInterview] datetime, 
	[RefuseDate] datetime, 
	[InterviewDate_1] datetime, 
	[Interviewer_1] nvarchar(255), 
	[At_1] nvarchar(255), 
	[InterviewDate_2] datetime, 
	[Interviewer_2] nvarchar(255), 
	[At_2] nvarchar(255), 
	[AskSupplier] datetime, 
	[AskTest] datetime, 
	[TestDate] datetime, 
	[TestAt] nvarchar(255), 
	[Notes] nvarchar(500), 
	[InterviewDate_3] datetime, 
	[Interviewer_3] nvarchar(255), 
	[At_3] nvarchar(255), 
	[InterviewDate_4] datetime, 
	[Interviewer_4] nvarchar(255), 
	[At_4] nvarchar(255), 
	FOREIGN KEY ([RequestID])
		REFERENCES [dbo].[tblCand_Request] ([RequestID])
		ON UPDATE CASCADE ON DELETE CASCADE
);
