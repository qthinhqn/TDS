	[DateChange] datetime, 
	[Amount] float, 
	[CurrType] nvarchar(20), 
	[SupplierID] nvarchar(50), 
	[Reason] nvarchar(500), 
	[Notes] nvarchar(500), 
	FOREIGN KEY ([RequestID])
		REFERENCES [dbo].[tblCand_Request] ([RequestID])
		ON UPDATE CASCADE ON DELETE CASCADE, 
	FOREIGN KEY ([FeeID])
		REFERENCES [dbo].[tblCandRef_Fee] ([FeeID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);
