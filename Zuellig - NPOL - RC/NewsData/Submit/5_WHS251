	[CurrType] nvarchar(20), 
	[PosID] nvarchar(10), 
	[ContractID] nvarchar(20), 
	[ContractTypeID] nvarchar(10), 
	[TempMonths] float, 
	[FromDate] datetime, 
	[ToDate] datetime, 
	[Recruited] bit, 
	[ContractPeriodID] nvarchar(10), 
	[LocationID] nvarchar(50), 
	[LocationID_Control] nvarchar(50), 
	[LocationID_Cost] nvarchar(50), 
	[LevelID] nvarchar(50), 
	[EmpID] int, 
	[Dep] nvarchar(25), 
	FOREIGN KEY ([CandID])
		REFERENCES [dbo].[tblCandidate] ([CandID])
		ON UPDATE CASCADE ON DELETE CASCADE
);


