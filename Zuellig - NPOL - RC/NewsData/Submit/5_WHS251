	[InterviewDate] datetime NOT NULL, 
	[Num] int NOT NULL, 
	[Notes] nvarchar(255),
	CONSTRAINT [PK_tblCand_Interview] PRIMARY KEY ([InterviewDate], [Num])
);


CREATE TABLE [dbo].[tblCand_Interview_Cand] (
	[InterviewDate] datetime NOT NULL, 
	[Num] int NOT NULL, 
	[CandID] nvarchar(25) NOT NULL, 
	[InterviewTime] datetime, 
	[EmployeeID] nvarchar(15), 
	[Interviewer] nvarchar(255), 
	[WrittingTime] datetime, 
	[InterviewFrom] datetime, 
	[InterviewTo] datetime, 
	[Content] nvarchar(2000), 
	[GeneralComment] nvarchar(2000), 
	[Recommend] nvarchar(2000), 
	[Write_Score] float, 
	[IT_Score] float, 
	[Interview_Score] float, 
	[Suitable_Score] float, 
	[Total_Score] float, 
	[OverallLevel] nvarchar(2000), 
	[ContentReject] nvarchar(2000), 
	[Place] nvarchar(255), 
	[SectionID] nvarchar(10), 
	[LineID] nvarchar(10), 
	[DepID] nvarchar(10), 
	[PosID] nvarchar(10), 
	[ExpectedSalary] nvarchar(150), 
	[StartDate] datetime, 
	[Probation_Sal] float, 
	[BasicSalary] float, 
	[IsContact] bit, 
	[IsInform] bit, 
	[IsAttend] bit, 
	[StatusID] nvarchar(10), 
	[Notes] nvarchar(2000), 
	[TotalTime] float, 
	[HumanComment] nvarchar(2000), 
	[HumanSuggestID] nvarchar(10), 
	[DeptHeadComment] nvarchar(2000), 
	[DeptHeadSuggestID] nvarchar(10), 
	[ManagerComment] nvarchar(2000), 
	[ManagerSuggestID] nvarchar(10), 
	[LevelID] nvarchar(10), 
	[LocationID] nvarchar(20), 
	[Question] nvarchar(2000), 
	[Answer] nvarchar(2000),
	CONSTRAINT [PK_tblCand_Interview_Cand] PRIMARY KEY ([InterviewDate], [Num], [CandID]), 
	FOREIGN KEY ([InterviewDate], [Num])
		REFERENCES [dbo].[tblCand_Interview] ([InterviewDate], [Num])
		ON UPDATE CASCADE ON DELETE CASCADE, 
	FOREIGN KEY ([CandID])
		REFERENCES [dbo].[tblCandidate] ([CandID])
		ON UPDATE CASCADE ON DELETE CASCADE
);
