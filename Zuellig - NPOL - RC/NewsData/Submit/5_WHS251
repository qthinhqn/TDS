CREATE TABLE [dbo].[tblEmpInsurance] (
	[EmployeeID] nvarchar(25) NOT NULL, 
	[DateChange] datetime NOT NULL, 
	[From_MY] nvarchar(7), 
	[To_MY] nvarchar(7), 
	[Amount] float, 
	[Notes] nvarchar(255),
	CONSTRAINT [PK_tblEmpInsurance] PRIMARY KEY ([EmployeeID], [DateChange])
);


CREATE TABLE [dbo].[tblEmpInsurance_Excel_Tmp] (
	[EmployeeID] nvarchar(25) NOT NULL, 
	[DateChange] datetime NOT NULL, 
	[From_MY] nvarchar(7), 
	[To_MY] nvarchar(7), 
	[Amount] float, 
	[Notes] nvarchar(255), 
	[IsDelete] bit, 
	[KeyID] int NOT NULL IDENTITY(1,1) PRIMARY KEY
);


CREATE TABLE [dbo].[tblRef_Language] (
	[LangID] nvarchar(10) NOT NULL PRIMARY KEY, 
	[LangName] nvarchar(50)
);


CREATE TABLE [dbo].[tblRef_LanguageLevel] (
	[LangLevelID] nvarchar(10) NOT NULL PRIMARY KEY, 
	[LangLevelName] nvarchar(50)
);


CREATE TABLE [dbo].[tblEmpLanguage] (
	[EmployeeID] nvarchar(25) NOT NULL, 
	[LangID] nvarchar(10) NOT NULL, 
	[LangLevelID] nvarchar(10), 
	[Speaking] nvarchar(50), 
	[Reading] nvarchar(50), 
	[Writing] nvarchar(50), 
	[Listening] nvarchar(50), 
	[Notes] nvarchar(100),
	CONSTRAINT [PK_tblEmpLanguage] PRIMARY KEY ([EmployeeID], [LangID]), 
	FOREIGN KEY ([LangID])
		REFERENCES [dbo].[tblRef_Language] ([LangID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([LangLevelID])
		REFERENCES [dbo].[tblRef_LanguageLevel] ([LangLevelID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([EmployeeID])
		REFERENCES [dbo].[tblCurriculumVitae] ([EmployeeID])
		ON UPDATE CASCADE ON DELETE CASCADE
);


CREATE TABLE [dbo].[tblEmpLateEarly] (
	[EmployeeID] nvarchar(25), 
	[ShiftID] nvarchar(10), 
	[Ngay_CC] datetime, 
	[D1] float, 
	[D2] float, 
	[D3] float, 
	[D] float, 
	[N1] float, 
	[N2] float, 
	[N3] float, 
	[N] float, 
	[Early_Day] int NOT NULL, 
	[Early_Night] int NOT NULL
);


CREATE TABLE [dbo].[tblRef_Level] (
	[LevelID] nvarchar(10) NOT NULL PRIMARY KEY, 
	[LevelName] nvarchar(100), 
	[LevelName_Japan] nvarchar(100), 
	[IsByHay] bit, 
	[Note] nvarchar(255)
);


CREATE TABLE [dbo].[tblEmpLevel] (
	[EmployeeID] nvarchar(25) NOT NULL, 
	[DateChange] datetime NOT NULL, 
	[LevelID] nvarchar(10) NOT NULL, 
	[Note] nvarchar(100),
	CONSTRAINT [PK_tblEmpLevel] PRIMARY KEY ([EmployeeID], [DateChange]), 
	FOREIGN KEY ([EmployeeID])
		REFERENCES [dbo].[tblEmployee] ([EmployeeID])
		ON UPDATE CASCADE ON DELETE CASCADE, 
	FOREIGN KEY ([LevelID])
		REFERENCES [dbo].[tblRef_Level] ([LevelID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);


CREATE TABLE [dbo].[tblEmpLevel_Excel_Tmp] (
	[EmployeeID] nvarchar(50), 
	[DateChange] datetime, 
	[LevelID] nvarchar(50), 
	[Note] nvarchar(100), 
	[IsDelete] bit, 
	[KeyID] int NOT NULL IDENTITY(1,1) PRIMARY KEY
);


CREATE TABLE [dbo].[tblEmpLevel_Tmp] (
	[EmployeeID] nvarchar(50), 
	[DateChange] datetime, 
	[LevelID] nvarchar(50), 
	[LevelName] nvarchar(255)
);


CREATE TABLE [dbo].[tblRef_Location] (
	[LocationID] nvarchar(20) NOT NULL PRIMARY KEY, 
	[LocationName] nvarchar(100), 
	[LocationName_Foreign] nvarchar(100), 
	[Address] nvarchar(255), 
	[Tel] nvarchar(50), 
	[Fax] nvarchar(50), 
	[Email] nvarchar(50), 
	[Notes] nvarchar(255)
);


CREATE TABLE [dbo].[tblEmpLocation] (
	[EmployeeID] nvarchar(25) NOT NULL, 
	[DateChange] datetime NOT NULL, 
	[EndDate] datetime, 
	[LocationID] nvarchar(20), 
	[Notes] nvarchar(255), 
	[IsBasedIn] bit, 
	[LocationID_Cost] nvarchar(20), 
	[LocationID_Control] nvarchar(20), 
	[KeyID] int NOT NULL PRIMARY KEY, 
	[DateProcess] datetime, 
	FOREIGN KEY ([LocationID])
		REFERENCES [dbo].[tblRef_Location] ([LocationID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([EmployeeID])
		REFERENCES [dbo].[tblEmployee] ([EmployeeID])
		ON UPDATE CASCADE ON DELETE CASCADE
);
CREATE UNIQUE NONCLUSTERED INDEX [IX_tblEmpLocation]
	ON [dbo].[tblEmpLocation] ([EmployeeID], [LocationID], [LocationID_Cost], [LocationID_Control]);


CREATE TABLE [dbo].[tblRef_LocationControl] (
	[LocationID] nvarchar(10) NOT NULL PRIMARY KEY, 
	[LocationName] nvarchar(100), 
	[Notes] nvarchar(255)
);


CREATE TABLE [dbo].[tblEmpLocation_Control] (
	[EmployeeID] nvarchar(25) NOT NULL, 
	[DateChange] datetime NOT NULL, 
	[LocationID] nvarchar(10), 
	[Notes] nvarchar(255),
	CONSTRAINT [PK_tblEmpLocation_Control] PRIMARY KEY ([EmployeeID], [DateChange]), 
	FOREIGN KEY ([LocationID])
		REFERENCES [dbo].[tblRef_LocationControl] ([LocationID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([EmployeeID])
		REFERENCES [dbo].[tblEmployee] ([EmployeeID])
		ON UPDATE CASCADE ON DELETE CASCADE
);


CREATE TABLE [dbo].[tblEmpLocation_Control_Excel_Tmp] (
	[EmployeeID] nvarchar(25), 
	[DateChange] datetime, 
	[LocationID] nvarchar(10), 
	[Notes] nvarchar(255), 
	[IsDelete] bit, 
	[KeyID] int NOT NULL IDENTITY(1,1) PRIMARY KEY
);


CREATE TABLE [dbo].[tblRef_LocationCost] (
	[LocationID] nvarchar(10) NOT NULL PRIMARY KEY, 
	[LocationName] nvarchar(100), 
	[Notes] nvarchar(255), 
	[BankFee] float
);


