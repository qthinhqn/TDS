CREATE TABLE [dbo].[tbl_TrainPropose] (
	[KeyID] int NOT NULL PRIMARY KEY, 
	[EmployeeID] nvarchar(25) NOT NULL, 
	[DateChange] datetime NOT NULL, 
	[CourseID] nvarchar(50), 
	[CourseName] nvarchar(255), 
	[TrainTime] nvarchar(50), 
	[TrainTypeID] nvarchar(10), 
	[FieldID] nvarchar(20), 
	[SkillID] nvarchar(20), 
	[Priority] int, 
	[Notes] nvarchar(255), 
	[IsDone] bit, 
	[IsFromPDR] bit, 
	[IsTransToPlan] bit, 
	FOREIGN KEY ([EmployeeID])
		REFERENCES [dbo].[tblEmployee] ([EmployeeID])
		ON UPDATE CASCADE ON DELETE CASCADE
);
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Ngày đánh giá',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainPropose',@level2type=N'Column',@level2name=N'DateChange';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Thời gian học',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainPropose',@level2type=N'Column',@level2name=N'TrainTime';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Mức độ ưu tiên',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainPropose',@level2type=N'Column',@level2name=N'Priority';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Đã thực hiện',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainPropose',@level2type=N'Column',@level2name=N'IsDone';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Lấy từ thông tin đánh giá',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainPropose',@level2type=N'Column',@level2name=N'IsFromPDR';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Dữ liệu này đã transfer qua tbl_TrainPlan chưa?',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainPropose',@level2type=N'Column',@level2name=N'IsTransToPlan';


