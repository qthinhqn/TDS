CREATE TABLE [dbo].[tblRef_NewsStatus] (
	[StatusID] char NOT NULL PRIMARY KEY, 
	[StatusName] nvarchar(50)
);
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'I, X, W, C, A, L, P, O, D',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tblRef_NewsStatus',@level2type=N'Column',@level2name=N'StatusID';


CREATE TABLE [dbo].[tblRef_NewsType] (
	[TypeID] int NOT NULL IDENTITY(1,1) PRIMARY KEY, 
	[TypeName] nvarchar(50)
);


CREATE TABLE [dbo].[tbl_News] (
	[NewsID] int NOT NULL IDENTITY(1,1) PRIMARY KEY, 
	[TypeID] int, 
	[Title] nvarchar(150), 
	[Summary] ntext, 
	[Content] ntext, 
	[Picture] nvarchar(300), 
	[UserID] nvarchar(25), 
	[CreationTime] smalldatetime, 
	[IssueDate] smalldatetime, 
	[IsAttach] bit, 
	[IsShow] bit, 
	[StatusID] char, 
	[ViewedCount] smallint DEFAULT (0), 
	[AllowSubmit] bit, 
	[DeadlineSubmit] smalldatetime, 
	FOREIGN KEY ([StatusID])
		REFERENCES [dbo].[tblRef_NewsStatus] ([StatusID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([TypeID])
		REFERENCES [dbo].[tblRef_NewsType] ([TypeID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);


CREATE TABLE [dbo].[tbl_Attachment] (
	[ID] int NOT NULL IDENTITY(1,1) PRIMARY KEY, 
	[AttachmentName] nvarchar(100), 
	[AttachmentPath] nvarchar(150), 
	[FileType] nvarchar(10), 
	[FileSize] varchar(30), 
	[NewsID] int, 
	FOREIGN KEY ([NewsID])
		REFERENCES [dbo].[tbl_News] ([NewsID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);


CREATE TABLE [dbo].[tbl_CalEmployee_Attendance_Tmp] (
	[KeyID] int NOT NULL IDENTITY(1,1), 
	[EmployeeID] nvarchar(20), 
	[Ngay_CC] datetime, 
	[PH] float, 
	[Sunday] int
);


