	[Type] nvarchar(50) NOT NULL, 
	[YearID] int NOT NULL, 
	[MonthID] int NOT NULL, 
	[EmployeeID] nvarchar(25) NOT NULL, 
	[EmployeeCode] nvarchar(50), 
	[EmployeeName] nvarchar(200), 
	[StoppedEmp] int, 
	[DepDirect] bit, 
	[EmployedDate] datetime, 
	[LeftDate] datetime, 
	[CompanyID] nvarchar(50), 
	[SectionID] nvarchar(10), 
	[SectionName] nvarchar(100), 
	[SectionOrderNo] int, 
	[LineID] nvarchar(10), 
	[LineName] nvarchar(100), 
	[LineOrderNo] int, 
	[DepID] nvarchar(10), 
	[DepName] nvarchar(100), 
	[DepOrderNo] int, 
	[PosID] nvarchar(10), 
	[PosName] nvarchar(100), 
	[PosOrderNo] int, 
	[PosRankID] nvarchar(50), 
	[RegionID] nvarchar(10), 
	[RegionName] nvarchar(100), 
	[SubRegionID] nvarchar(10), 
	[SubRegionName] nvarchar(100), 
	[Email] nvarchar(255), 
	[PayslipPassword] nvarchar(255), 
	[IsSend] bit, 
	[SendDate] datetime, 
	[SendBy] nvarchar(1000), 
	[Status] nvarchar(1000), 
	[Note] nvarchar(4000),
	CONSTRAINT [PK_tblPayroll_SendMail] PRIMARY KEY ([Type], [YearID], [MonthID], [EmployeeID]), 
	FOREIGN KEY ([EmployeeID])
		REFERENCES [dbo].[tblEmployee] ([EmployeeID])
		ON UPDATE CASCADE ON DELETE CASCADE
);


