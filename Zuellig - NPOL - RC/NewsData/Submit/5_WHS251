EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Báo cáo đến ',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainPlan',@level2type=N'Column',@level2name=N'EmailTo';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Lấy từ thông tin đánh giá',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainPlan',@level2type=N'Column',@level2name=N'IsFromPDR';


CREATE TABLE [dbo].[tbl_TrainPlan_Emp] (
	[DateChange] datetime NOT NULL, 
	[CourseName] nvarchar(255) NOT NULL, 
	[EmployeeID] nvarchar(25) NOT NULL,
	CONSTRAINT [PK_tbl_TrainPlan_Emp] PRIMARY KEY ([DateChange], [CourseName], [EmployeeID]), 
	FOREIGN KEY ([EmployeeID])
		REFERENCES [dbo].[tblEmployee] ([EmployeeID])
		ON UPDATE CASCADE ON DELETE CASCADE, 
	FOREIGN KEY ([CourseName], [DateChange])
		REFERENCES [dbo].[tbl_TrainPlan] ([CourseName], [DateChange])
		ON UPDATE CASCADE ON DELETE CASCADE
);


CREATE TABLE [dbo].[tbl_TrainPlan_Excel_Tmp] (
	[EmployeeID] nvarchar(25), 
	[DateChange] datetime, 
