CREATE TABLE [dbo].[tblRef_EmployeeType] (
	[EmpTypeID] nvarchar(5) NOT NULL PRIMARY KEY, 
	[EmpTypeName] nvarchar(100), 
	[EmpTypeName_Japan] nvarchar(200), 
	[OrderNo] int, 
	[IsDirect] bit
);
CREATE TRIGGER [dbo].[tblRef_EmployeeType_UTrig] ON dbo.tblRef_EmployeeType 
FOR UPDATE AS
SET NOCOUNT ON
/* * CASCADE UPDATES TO 'tblCurriculumViate' */
IF UPDATE(EmpTypeID)
    BEGIN
       UPDATE tblEmployee
       SET tblEmployee.EmpTypeID = inserted.EmpTypeID
       FROM tblEmployee, deleted, inserted
       WHERE deleted.EmpTypeID = tblEmployee.EmpTypeID
   END

IF UPDATE(EmpTypeName)
    BEGIN
       UPDATE tblEmployee
       SET tblEmployee.EmpTypeName = inserted.EmpTypeName
       FROM tblEmployee, deleted, inserted
       WHERE deleted.EmpTypeName = tblEmployee.EmpTypeName
   END;


CREATE TABLE [dbo].[tblEmpType] (
	[EmployeeID] nvarchar(25) NOT NULL, 
	[DateChange] datetime NOT NULL, 
	[EmpTypeID] nvarchar(5), 
	[Notes] nvarchar(100),
	CONSTRAINT [PK_tblEmpType] PRIMARY KEY ([EmployeeID], [DateChange]), 
	FOREIGN KEY ([EmpTypeID])
		REFERENCES [dbo].[tblRef_EmployeeType] ([EmpTypeID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([EmployeeID])
		REFERENCES [dbo].[tblEmployee] ([EmployeeID])
		ON UPDATE CASCADE ON DELETE CASCADE
);


