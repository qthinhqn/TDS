CREATE TABLE [dbo].[tblResultDetail] (
	[EmployeeID] nvarchar(25), 
	[PunchDate] datetime, 
	[ShiftID] nvarchar(10), 
	[WD] float, 
	[WN] float, 
	[WPH] float, 
	[PaidDay] float, 
	[PaidRate] float, 
	[WLDay] float, 
	[WLRate] float, 
	[PerTimeID] nvarchar(10), 
	[LeaveID] nvarchar(10), 
	[LateMin] float, 
	[RealLateMin] float, 
	[EarlyMin] float, 
	[RealEarlyMin] float, 
	[GoOutMin] float, 
	[RealGoOutMin] float, 
	[OT15] float, 
	[OT15_Before] float, 
	[OT15_After] float, 
	[OT15Night] float, 
	[OT15Night_Before] float, 
	[OT15Night_After] float, 
	[OT20] float, 
	[OT20Night] float, 
	[OT30] float, 
	[OT30Night] float, 
	[OTSatNight] float, 
	[OTSunNight] float, 
	[Ngay_CC] datetime, 
	[PunchIn] datetime, 
	[PunchOut] datetime, 
	[Paid_Days] float, 
	[WL_Days] float, 
	[Work_Days] float, 
	[STT] int, 
	[LeaveName] nvarchar(100), 
	[OT100] float, 
	[OT100_Acc] float, 
	[IsNoScan] bit, 
	[IsDayoff] bit, 
	[KeyID] int NOT NULL PRIMARY KEY, 
	[IsDiligence] bit, 
	[ModeID] nvarchar(25), 
	[RealIn] datetime, 
	[RealOut] datetime, 
	[Rate] float, 
	[WN_Days] float, 
	[Meals] float, 
