CREATE TABLE [dbo].[tblEmpLunch] (
	[KeyID] int NOT NULL PRIMARY KEY, 
	[SectionID] nvarchar(10), 
	[PartID] nvarchar(10), 
	[TeamID] nvarchar(10), 
	[GroupID] nvarchar(10), 
	[LineID] nvarchar(10), 
	[DateChange] datetime, 
	[MealTypeID] nvarchar(10), 
	[TotalPerson] int, 
	[TotalMeal] int, 
	[TotalMeal_Foreign] int, 
	[VegPerson] int, 
	[Notes] nvarchar(100), 
	FOREIGN KEY ([MealTypeID])
		REFERENCES [dbo].[tblRef_MealType] ([MealTypeID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);
CREATE NONCLUSTERED INDEX [IX_tblEmpLunch]
	ON [dbo].[tblEmpLunch] ([SectionID], [LineID], [GroupID], [TeamID], [PartID], [DateChange]);


