	[PlaceOfBirth] nvarchar(100), 
	[IDCard] nvarchar(50), 
	[IssuePlace] nvarchar(100), 
	[IssueDate] datetime, 
	[Passport] nvarchar(50), 
	[IssuseDate_PP] datetime, 
	[ExpireDate_PP] datetime, 
	[IssusePlace_PP] nvarchar(150), 
	[SexID] nvarchar(1), 
	[MaritalStatusID] nvarchar(10), 
	[Phone] nvarchar(50), 
	[Pager] nvarchar(10), 
	[Mobile] nvarchar(50), 
	[Address] nvarchar(150), 
	[HouseNo] nvarchar(255), 
	[Street] nvarchar(150), 
	[Ward] nvarchar(50), 
	[DistrictID] nvarchar(10), 
	[DistrictName] nvarchar(50), 
	[CityID] nvarchar(10), 
	[CityName] nvarchar(50), 
	[Address_Temp] nvarchar(150), 
	[HouseNo_Temp] nvarchar(255), 
	[Street_Temp] nvarchar(150), 
	[Ward_Temp] nvarchar(50), 
	[DistrictID_Temp] nvarchar(10), 
	[DistrictName_Temp] nvarchar(50), 
	[CityID_Temp] nvarchar(10), 
	[CityName_Temp] nvarchar(50), 
	[NationalityID] nvarchar(10), 
	[ReligionID] nvarchar(10), 
	[RaceID] nvarchar(10), 
	[QualificationID] nvarchar(10), 
	[EducationID] nvarchar(10), 
	[NativeCountry] nvarchar(100), 
	[Photo] nvarchar(255), 
	[RoadFrom] float, 
	[Aptitude] nvarchar(50), 
	[TotalKid] tinyint, 
	[Family] nvarchar(50), 
	[Alias] nvarchar(50), 
	[Visa] nvarchar(50), 
	[TempAddressCard] nvarchar(50), 
	[Party_Date] datetime, 
	[Party_DateFormal] datetime, 
	[Party_Place] nvarchar(100), 
	[Party_Position] nvarchar(50), 
	[Union_Date] datetime, 
	[Union_Place] nvarchar(100), 
	[Army_DateIn] datetime, 
	[Army_DateOut] datetime, 
	[Army_Position] nvarchar(100), 
	[Army_Division] nvarchar(100), 
	[CompanyID] nvarchar(50), 
	FOREIGN KEY ([MaritalStatusID])
		REFERENCES [dbo].[tblRef_MaritalStatus] ([MaritalStatusID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([SexID])
		REFERENCES [dbo].[tblRef_Sex] ([SexID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([QualificationID])
		REFERENCES [dbo].[tblRef_Qualification] ([QualificationID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([NationalityID])
		REFERENCES [dbo].[tblRef_Nationality] ([NationalityID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([RaceID])
		REFERENCES [dbo].[tblRef_Race] ([RaceID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([EmployeeID])
		REFERENCES [dbo].[tblEmployee] ([EmployeeID])
		ON UPDATE CASCADE ON DELETE CASCADE, 
	FOREIGN KEY ([ReligionID])
		REFERENCES [dbo].[tblRef_Religion] ([ReligionID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);
