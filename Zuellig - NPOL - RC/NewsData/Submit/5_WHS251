	[TicketID] nvarchar(20) NOT NULL PRIMARY KEY, 
	[DateChange] datetime, 
	[TypeID] nvarchar(10), 
	[SupplierID] nvarchar(50), 
	[InformID] nvarchar(20), 
	[FromDate] datetime, 
	[ToDate] datetime, 
	[DeadLine] datetime, 
	[Content] nvarchar(500), 
	[TotalFee] float, 
	[CurrType] nvarchar(20), 
	[Notes] nvarchar(255), 
