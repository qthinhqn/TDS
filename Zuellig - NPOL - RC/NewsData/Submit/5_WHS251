	[OT_After] float
);


CREATE TABLE [dbo].[tbl_EmpOT_Registry] (
	[EmployeeID] nvarchar(15) NOT NULL, 
	[DateChange] datetime NOT NULL, 
	[FromTime] datetime NOT NULL, 
	[DayTotalHours] float, 
	[NightTotalHours] float, 
	[OT_Night] float, 
	[OT_Day] float, 
	[Sunday] bit NOT NULL, 
	[PH] bit NOT NULL, 
	[OTDay_Before] numeric(11, 1), 
	[OTDay_After] numeric(11, 1), 
	[OTNight_Before] numeric(1, 1) NOT NULL, 
	[OTNight_After] numeric(1, 1) NOT NULL, 
	[OTD_Before] numeric(11, 1), 
	[OTD_After] numeric(11, 1), 
	[OTN_Before] numeric(11, 1), 
	[OTN_After] numeric(11, 1)
);


CREATE TABLE [dbo].[tbl_EmpOT_Result] (
	[EmployeeID] nvarchar(15) NOT NULL, 
	[DateChange] datetime NOT NULL, 
	[FromTime] datetime NOT NULL, 
