CREATE TABLE [dbo].[tbl_TrainRequest_Fee] (
	[RequestID] nvarchar(50) NOT NULL, 
	[TypeID] nvarchar(10) NOT NULL, 
	[Amount] float, 
	[Notes] nvarchar(255), 
	[CurrType] nvarchar(20),
	CONSTRAINT [PK_tbl_TrainRequest_Fee] PRIMARY KEY ([RequestID], [TypeID]), 
	FOREIGN KEY ([TypeID])
		REFERENCES [dbo].[tbl_TrainRef_Fee] ([TypeID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([RequestID])
		REFERENCES [dbo].[tbl_TrainRequest] ([RequestID])
		ON UPDATE CASCADE ON DELETE CASCADE
);


CREATE TABLE [dbo].[tbl_TrainRequest_KeepBonus] (
	[RequestID] nvarchar(50) NOT NULL, 
	[MonthID] int NOT NULL, 
	[YearID] int NOT NULL, 
	[Rate] float, 
	[Note] nvarchar(255),
	CONSTRAINT [PK_tbl_TrainRequest_KeepBonus] PRIMARY KEY ([RequestID], [MonthID], [YearID]), 
	FOREIGN KEY ([RequestID])
		REFERENCES [dbo].[tbl_TrainRequest] ([RequestID])
		ON UPDATE CASCADE ON DELETE CASCADE
);
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Tỷ lệ giữ lại tiền thưởng (50% or 30%, ...). Người sd sẽ nhập 50, 30, ...',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainRequest_KeepBonus',@level2type=N'Column',@level2name=N'Rate';


CREATE TABLE [dbo].[tbl_TrainRequest_Paid] (
	[RequestID] nvarchar(50) NOT NULL, 
	[Round] int NOT NULL, 
	[DateChange] datetime, 
	[Amount] float, 
	[IsPaid] bit, 
	[Notes] nvarchar(500), 
	[ExRate] float,
	CONSTRAINT [PK_tbl_TrainRequest_Paid] PRIMARY KEY ([RequestID], [Round]), 
	FOREIGN KEY ([RequestID])
		REFERENCES [dbo].[tbl_TrainRequest] ([RequestID])
		ON UPDATE CASCADE ON DELETE CASCADE
);
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Đợt thanh toán',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainRequest_Paid',@level2type=N'Column',@level2name=N'Round';


CREATE TABLE [dbo].[tbl_TrainRequest_Refund] (
	[RequestID] nvarchar(50) NOT NULL, 
	[FromDate] datetime NOT NULL, 
	[ToDate] datetime, 
	[Rate] float, 
	[Notes] nvarchar(255),
	CONSTRAINT [PK_tbl_TrainRequest_Refund] PRIMARY KEY ([RequestID], [FromDate]), 
	FOREIGN KEY ([RequestID])
		REFERENCES [dbo].[tbl_TrainRequest] ([RequestID])
		ON UPDATE CASCADE ON DELETE CASCADE
);


CREATE TABLE [dbo].[tbl_TrainSupplier_Contact] (
	[KeyID] int NOT NULL PRIMARY KEY, 
	[SupplierID] nvarchar(20), 
	[RegionID] nvarchar(20), 
	[Title] nvarchar(10), 
	[ContactName] nvarchar(60), 
	[PosName] nvarchar(100), 
	[Extension] nvarchar(100), 
	[Mobile] nvarchar(50), 
	[Email] nvarchar(150), 
	[Notes] nvarchar(255), 
	FOREIGN KEY ([SupplierID])
		REFERENCES [dbo].[tbl_TrainSupplier] ([SupplierID])
		ON UPDATE CASCADE ON DELETE CASCADE
);


CREATE TABLE [dbo].[tbl_TrainSupplier_Contract] (
	[SupplierID] nvarchar(20) NOT NULL, 
	[ContractID] nvarchar(50) NOT NULL, 
	[DateChange] datetime NOT NULL, 
	[Signer] nvarchar(100), 
	[StartDate] datetime, 
	[EndDate] datetime, 
	[Content] nvarchar(4000), 
	[Fee] float, 
	[CurrType] nvarchar(20), 
	[Notes] nvarchar(500),
	CONSTRAINT [PK_tbl_TrainSupplierContract] PRIMARY KEY ([SupplierID], [ContractID]), 
	FOREIGN KEY ([SupplierID])
		REFERENCES [dbo].[tbl_TrainSupplier] ([SupplierID])
		ON UPDATE CASCADE ON DELETE CASCADE
);
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Ngày ký hợp đồng',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainSupplier_Contract',@level2type=N'Column',@level2name=N'DateChange';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Người ký hợp đồng',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainSupplier_Contract',@level2type=N'Column',@level2name=N'Signer';


CREATE TABLE [dbo].[tbl_TrainSupplier_Course] (
	[KeyID] int NOT NULL PRIMARY KEY, 
	[SupplierID] nvarchar(20), 
	[CourseID] nvarchar(50), 
	[CourseName] nvarchar(255), 
	[CourseName_Eng] nvarchar(255), 
	[FieldID] nvarchar(20), 
	[Fee] float, 
	[CurrType] nvarchar(20), 
	[Start] nvarchar(50), 
	[Notes] nvarchar(255), 
	FOREIGN KEY ([SupplierID])
		REFERENCES [dbo].[tbl_TrainSupplier] ([SupplierID])
		ON UPDATE CASCADE ON DELETE CASCADE
);


CREATE TABLE [dbo].[tbl_TrainSupplier_Field] (
	[SupplierID] nvarchar(20) NOT NULL, 
	[FieldID] nvarchar(20) NOT NULL, 
	[Notes] nvarchar(255),
	CONSTRAINT [PK_tbl_TrainSupplier_Field] PRIMARY KEY ([SupplierID], [FieldID]), 
	FOREIGN KEY ([SupplierID])
		REFERENCES [dbo].[tbl_TrainSupplier] ([SupplierID])
		ON UPDATE CASCADE ON DELETE CASCADE, 
	FOREIGN KEY ([FieldID])
		REFERENCES [dbo].[tbl_TrainRef_Field] ([FieldID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);


