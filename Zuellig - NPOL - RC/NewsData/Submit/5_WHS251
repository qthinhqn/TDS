EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Chi phí công ty',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainPlan',@level2type=N'Column',@level2name=N'CompFee';
