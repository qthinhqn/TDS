CREATE TABLE [dbo].[tblRef_GroupRank] (
	[DateChange] datetime NOT NULL, 
	[GroupID] nvarchar(10) NOT NULL, 
	[RankID] nvarchar(10) NOT NULL, 
	[Salary] float, 
	[Trial] float, 
	[Note] nvarchar(255),
	CONSTRAINT [PK_tblRef_GroupRank] PRIMARY KEY ([DateChange], [GroupID], [RankID]), 
	FOREIGN KEY ([GroupID])
		REFERENCES [dbo].[tblRef_Group] ([GroupID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([RankID])
		REFERENCES [dbo].[tblRef_Rank] ([RankID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);


CREATE TABLE [dbo].[tblRef_HIPlace] (
	[KeyID] int NOT NULL IDENTITY(1,1) PRIMARY KEY, 
	[HIPlace] nvarchar(100), 
	[Notes] nvarchar(100), 
	[HIPlaceID] nvarchar(10), 
	[DistrictID] nvarchar(20)
);
CREATE TRIGGER [dbo].[tblHIPlace_UTrig] ON  dbo.tblRef_HIPlace FOR UPDATE AS
SET NOCOUNT ON
/* * CASCADE UPDATES TO 'tblEmpHIPlace' */
IF UPDATE(HIPlace)
    BEGIN
       UPDATE  tblEmpHIPlace
       SET  tblEmpHIPlace.HIPlace = inserted.HIPlace
       FROM  tblEmpHIPlace, deleted, inserted
       WHERE deleted.HIPlace =  tblEmpHIPlace.HIPlace	
    END;


CREATE TABLE [dbo].[tblRef_HIPlace_Backup] (
