	[PosID] nvarchar(25), 
	[RegionID] nvarchar(25), 
	[Numof] int, 
	[TypeID] nvarchar(10), 
	[ReasonID] nvarchar(10), 
	[EmpID_Replace] nvarchar(25), 
	[EmpReplace_Sal] float, 
	[EmpReplace_Allowance] float, 
	[IsInternalPost] bit, 
	[LocationID] nvarchar(20), 
	[SexID] nvarchar(25), 
	[JobDes] bit, 
	[JobDes_File] nvarchar(255), 
	[From_LineID] nvarchar(25), 
	[From_DepID] nvarchar(25), 
	[From_LocationID] nvarchar(20), 
	[IsBudgetHead] bit, 
	[StartDate] datetime, 
	[ProbationID] nvarchar(10), 
	[Probation_Sal] float, 
	[Probation_Travel] float, 
	[Probation_Allowance] float, 
	[Permanent_Sal] float, 
	[Permanent_Travel] float, 
	[Permanent_Allowance] float, 
	[Requester] nvarchar(100), 
	[ReportTo] nvarchar(100), 
	[ApprovedBy] nvarchar(100), 
	[ApprovedDate] datetime, 
	[HRDept_Note] nvarchar(500), 
	[ContractID] nvarchar(50), 
	FOREIGN KEY ([ReasonID])
		REFERENCES [dbo].[tblCandRef_Reason] ([ReasonID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([TypeID])
		REFERENCES [dbo].[tblCandRef_RecruitType] ([TypeID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([RecruitByID])
		REFERENCES [dbo].[tblCandRef_RecruitBy] ([RecruitByID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);
