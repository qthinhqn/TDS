CREATE TABLE [dbo].[tblRole] (
	[RoleID] nvarchar(50) NOT NULL PRIMARY KEY, 
	[RoleName] nvarchar(100), 
	[Notes] nvarchar(100)
);


CREATE TABLE [dbo].[tblRoleGroup] (
	[RoleID] nvarchar(50) NOT NULL, 
	[GroupID] nvarchar(50) NOT NULL,
	CONSTRAINT [PK_tblRoleGroup] PRIMARY KEY ([RoleID], [GroupID]), 
	FOREIGN KEY ([GroupID])
		REFERENCES [dbo].[tblGroup] ([GroupID])
		ON UPDATE CASCADE ON DELETE CASCADE, 
	FOREIGN KEY ([RoleID])
		REFERENCES [dbo].[tblRole] ([RoleID])
		ON UPDATE CASCADE ON DELETE CASCADE
);


CREATE TABLE [dbo].[tblRoleGroupObject] (
	[RoleID] nvarchar(50) NOT NULL, 
	[GroupID] nvarchar(50) NOT NULL, 
	[ObjectID] nvarchar(100) NOT NULL, 
	[RoleView] bit NOT NULL, 
	[RoleAdd] bit NOT NULL, 
	[RoleEdit] bit NOT NULL, 
	[RoleDelete] bit NOT NULL,
	CONSTRAINT [PK_tblRoleGroupObject] PRIMARY KEY ([RoleID], [GroupID], [ObjectID]), 
	FOREIGN KEY ([GroupID], [RoleID])
		REFERENCES [dbo].[tblRoleGroup] ([GroupID], [RoleID])
		ON UPDATE CASCADE ON DELETE CASCADE
);


CREATE TABLE [dbo].[tblRoleGroupObject_Temp] (
	[ObjectID] nvarchar(100) NOT NULL, 
	[GroupID] nvarchar(50) NOT NULL, 
	[RoleID] nvarchar(50) NOT NULL, 
	[ObjectName] nvarchar(100), 
	[RoleAdd] bit NOT NULL, 
	[RoleEdit] bit NOT NULL, 
	[RoleDelete] bit NOT NULL, 
	[RoleView] bit NOT NULL
);


CREATE TABLE [dbo].[tblRoleGroupObjectDetail] (
	[RoleID] nvarchar(50) NOT NULL, 
	[GroupID] nvarchar(50) NOT NULL, 
	[ObjectID] nvarchar(100) NOT NULL, 
	[RoleView] bit NOT NULL, 
	[RoleAdd] bit NOT NULL, 
	[RoleEdit] bit NOT NULL, 
	[RoleDelete] bit NOT NULL,
	CONSTRAINT [PK_tblRoleGroupObjectDetail] PRIMARY KEY ([RoleID], [GroupID], [ObjectID]), 
	FOREIGN KEY ([GroupID], [RoleID])
		REFERENCES [dbo].[tblRoleGroup] ([GroupID], [RoleID])
		ON UPDATE CASCADE ON DELETE CASCADE
);


