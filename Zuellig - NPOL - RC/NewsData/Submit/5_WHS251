	[EmployedDate] datetime, 
	[CompanyID] nvarchar(10), 
	[EmpTypeID_Sub] nvarchar(10), 
	[EmpTypeID] nvarchar(10), 
	[RegionID] nvarchar(10), 
	[SectionID] nvarchar(10), 
	[LineID] nvarchar(10), 
