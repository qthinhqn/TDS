	[LastName] nvarchar(50), 
	[SectionID] nvarchar(10), 
	[LineID] nvarchar(10), 
	[DepID] nvarchar(10), 
	[TeamID] nvarchar(10), 
	[PartID] nvarchar(10), 
	[IsHide] bit,
	CONSTRAINT [PK_tblRecorder_Backup] PRIMARY KEY ([Ngay_CC], [EmployeeID], [SweepIn]), 
	FOREIGN KEY ([EmployeeID])
		REFERENCES [dbo].[tblEmployee] ([EmployeeID])
		ON UPDATE CASCADE ON DELETE NO ACTION
