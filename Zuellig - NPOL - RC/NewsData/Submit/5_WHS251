CREATE TABLE [dbo].[tblEmpDayOff] (
	[KeyID] int NOT NULL PRIMARY KEY, 
	[EmployeeID] nvarchar(25) NOT NULL, 
	[FromDate] datetime, 
	[ToDate] datetime, 
	[StartDate] datetime, 
	[Days] float, 
	[LeaveID] nvarchar(10) NOT NULL, 
	[PerTimeID] nvarchar(1) NOT NULL DEFAULT (0), 
	[Notes] nvarchar(500), 
	[CompForDate] datetime, 
	[ApprovedDate] datetime, 
	[RegistedDate] datetime, 
	[Start_MY] nvarchar(7), 
	[End_MY] nvarchar(7), 
	[FromTime] datetime, 
	[ToTime] datetime, 
	[Preg_FromMY] nvarchar(7), 
	[Preg_ToMY] nvarchar(7), 
	[Preg_Start7h] datetime, 
	[Preg_IsLate] bit, 
	[Child_FromDate] datetime, 
	[Child_ToDate] datetime, 
	[ChildName] nvarchar(100), 
	[Is7h] bit, 
	[IsLate] bit, 
	[IsMaternity_PaidByComp] bit, 
	[ReceivedDate] datetime, 
	[IsMaternity_LimitSal] bit, 
	[IsPaid_ByCompany] bit, 
	[IsAdd_BySystem] bit, 
	[DateProcess] datetime, 
	FOREIGN KEY ([EmployeeID])
		REFERENCES [dbo].[tblEmployee] ([EmployeeID])
		ON UPDATE CASCADE ON DELETE CASCADE
);
