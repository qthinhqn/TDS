CREATE TRIGGER [dbo].[tblEmployee_UTrig] ON dbo.tblEmployee 
FOR UPDATE 
AS--Begin
SET NOCOUNT ON
/* * CASCADE UPDATES TO 'tblEmpRelationship' */
