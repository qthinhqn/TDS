CREATE TABLE [dbo].[tblRef_Degree] (
	[DegreeID] nvarchar(10) NOT NULL PRIMARY KEY, 
	[DegreeName] nvarchar(100)
);


CREATE TABLE [dbo].[tblRef_Education] (
	[EducationID] nvarchar(10) NOT NULL PRIMARY KEY, 
	[EducationType] nvarchar(100), 
	[EducationType_Foreign] nvarchar(100), 
	[BasicSal] float
);
CREATE TRIGGER [dbo].[tblRef_Education_DTrig] ON dbo.tblRef_Education
FOR DELETE 
AS
/* * PREVENT DELETES IF DEPENDENT RECORDS IN 'tblCurriculumVitae' */
IF (SELECT COUNT(*) FROM deleted, tblCurriculumVitae WHERE (deleted.EducationID = tblCurriculumVitae.EducationID)) > 0
    BEGIN
        RAISERROR 44445 'You can not delete this data because you use it in Curriculum Vitae !' --'The record can''t be deleted or changed. Since related records exist in table ''tblCurriculumVitae'', referential integrity rules would be violated.'
        ROLLBACK TRANSACTION
    END;
CREATE TRIGGER [dbo].[tblRef_Education_UTrig] ON dbo.tblRef_Education FOR UPDATE AS
SET NOCOUNT ON
/* * CASCADE UPDATES TO 'tblCurriculumViate' , tblCandEducation*/
IF UPDATE(EducationID)
    BEGIN
       UPDATE tblCurriculumVitae
       SET tblCurriculumVitae.EducationID = inserted.EducationID
       FROM tblCurriculumVitae, deleted, inserted
       WHERE deleted.EducationID = tblCurriculumVitae.EducationID	

      
    END;


CREATE TABLE [dbo].[tblEmpEducation] (
	[EmployeeID] nvarchar(25) NOT NULL, 
	[FromDate] nvarchar(50), 
	[ToDate] nvarchar(50), 
	[School] nvarchar(100), 
	[Subjects] nvarchar(100), 
	[DegreeID] nvarchar(10), 
	[DegreeCode] nvarchar(20), 
	[EducationID] nvarchar(10), 
	[YearInSchool] nvarchar(20), 
	[GraduationYear] nvarchar(20), 
	[KeyID] int NOT NULL PRIMARY KEY, 
	FOREIGN KEY ([EmployeeID])
		REFERENCES [dbo].[tblCurriculumVitae] ([EmployeeID])
		ON UPDATE CASCADE ON DELETE CASCADE, 
	FOREIGN KEY ([EducationID])
		REFERENCES [dbo].[tblRef_Education] ([EducationID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([DegreeID])
		REFERENCES [dbo].[tblRef_Degree] ([DegreeID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);
