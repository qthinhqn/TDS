CREATE TABLE [dbo].[tblEmployee] (
	[EmployeeID] nvarchar(25) NOT NULL PRIMARY KEY, 
	[EmployeeCode] nvarchar(25), 
	[FirstName] nvarchar(20), 
	[LastName] nvarchar(40), 
	[EmployeeName] nvarchar(60), 
	[EmployeeName_Foreign] nvarchar(150), 
	[CardID] nvarchar(14), 
	[EmpTypeID] nvarchar(10), 
	[EmpTypeName] nvarchar(100), 
	[EmployedDate] datetime, 
	[ContractTypeName] nvarchar(100), 
	[ContractDate] datetime, 
	[TempMonth] float, 
	[TempEndDate] datetime, 
	[SI] bit, 
	[SINo] nvarchar(50), 
	[SIDate] datetime, 
	[SIFrom_MY] datetime, 
	[SIPlace] nvarchar(150), 
	[SINo_Old] nvarchar(50), 
	[SINo_DateChange] datetime, 
	[SITo_MY] datetime, 
	[Reason_Reserve] nvarchar(150), 
	[TransferToPlace] nvarchar(150), 
	[TransferSIBook] bit, 
	[ReserveSIBook] bit, 
	[UIDate] datetime, 
	[IssuedSI] bit, 
	[HI] bit, 
	[HINo] nvarchar(50), 
	[HIDate] datetime, 
	[HIIssueAt] nvarchar(150), 
	[HIFrom_MY] datetime, 
	[HIPlace] nvarchar(150), 
	[Labor] bit, 
	[LaborNo] nvarchar(50), 
	[LaborDate] datetime, 
	[LaborPlace] nvarchar(150), 
	[TradeUnionMember] bit, 
	[TradeUnionDate] datetime, 
	[TradeUnionFee] float, 
	[SubmitDate] datetime, 
	[LeftDate] datetime, 
	[LeftTypeID] nvarchar(10), 
	[LeftTypeName] nvarchar(100), 
	[ReasonForLeft] nvarchar(150), 
	[LeftTypeID_Real] nvarchar(10), 
	[DecisionNo] nvarchar(50), 
	[TransferReportNo] nvarchar(50), 
	[FinalPaymentDate] datetime, 
	[HasFinalPayment] bit, 
	[FinalPaymentMonth] int, 
	[FinalPaymentYear] int, 
	[LastPayrollMonth] int, 
	[LastPayrollYear] int, 
	[Email] nvarchar(50), 
	[Notes] nvarchar(255), 
	[EmployeeName_Eng] nvarchar(60), 
	[BankAccount] nvarchar(50), 
	[BankAccount_USD] nvarchar(50), 
	[BankID] nvarchar(50), 
	[HasATMCard] bit, 
	[ATMDate] datetime, 
	[RegionID] nvarchar(10), 
	[RegionName] nvarchar(100), 
	[SubRegionID] nvarchar(10), 
	[SubRegionName] nvarchar(100), 
	[CompanyID] nvarchar(10), 
	[CompanyName] nvarchar(100), 
	[SectionID] nvarchar(10), 
	[SectionName] nvarchar(100), 
	[LineID] nvarchar(10), 
	[LineName] nvarchar(100), 
	[DepID] nvarchar(10), 
	[DepName] nvarchar(100), 
	[TeamID] nvarchar(10), 
	[TeamName] nvarchar(100), 
	[PartID] nvarchar(10), 
	[PartName] nvarchar(100), 
	[PosID] nvarchar(10), 
	[PosName] nvarchar(100), 
	[BasicSalary] float, 
	[SalLevelID] nvarchar(10), 
	[SalaryTypeName] nvarchar(50), 
	[BasicSalaryUSD] float, 
	[RegularAllowance] float, 
	[RegularAllowanceUSD] float, 
	[WD] tinyint, 
	[AppliedDate] datetime, 
	[Heath_Date] datetime, 
	[Heath_Result] nvarchar(150), 
	[OldEmployeeID] nvarchar(25), 
	[PITCode] nvarchar(50), 
	[Birthday] nvarchar(10), 
	[MaritalStatusID] nvarchar(10), 
	[SexID] nvarchar(10), 
	[IDCard] nvarchar(20), 
	[IssuePlace] nvarchar(150), 
	[IssueDate] datetime, 
	[Address] nvarchar(255), 
	[Phone] nvarchar(50), 
	[Mobile] nvarchar(50), 
	[StatusID] nvarchar(10), 
	[Introducer] nvarchar(50), 
	[Introducer_EmpID] nvarchar(25), 
	[Introducer_Dep] nvarchar(100), 
	[IntroducerRelation] nvarchar(255), 
	[MotorbikeNo] nvarchar(20), 
	[SecurityID] nvarchar(20), 
	[WorkingLicense] nvarchar(50), 
	[QualificationID] nvarchar(10), 
	[EducationID] nvarchar(10), 
	[DegreeID] nvarchar(10), 
	[IssuePlace_ID] nvarchar(10), 
	[RecruitedDate] datetime, 
	[GiveUpJob_SendDate] datetime, 
	[DisciplineDate] datetime, 
	[LeftTypeID_Submit] nvarchar(25), 
	[LeftTypeName_Submit] nvarchar(225), 
	[IsForeigner] bit, 
	[IsMaternity] bit, 
	[IsNotYearlyHealth] bit, 
	[IsFromRecruit] bit, 
	[IsNotOT] bit, 
	[IsNotScand] bit, 
	[FileNo] nvarchar(50), 
	[SupervisorID] nvarchar(25), 
	[ReportTo] nvarchar(255), 
	[FineDays] float, 
	[For_FY] nvarchar(7), 
	[IsAdd_Budget] bit, 
	[MY_SaveProfile] nvarchar(7), 
	[CoduphaDate] datetime, 
	[InterestConflict] nvarchar(500), 
	[HasPayroll13] bit, 
	[IsNotCalPayroll] bit, 
	[IsHoldSalary] bit, 
	[PayslipEmail] nvarchar(100), 
	[PayslipPassword] nvarchar(100), 
	[AutoPass] bit, 
	FOREIGN KEY ([LeftTypeID_Submit])
		REFERENCES [dbo].[tblRef_LeftType_Submit] ([LeftTypeID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([LeftTypeID_Real])
		REFERENCES [dbo].[tblRef_LeftType_Real] ([LeftTypeID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([BankID])
		REFERENCES [dbo].[tblRef_Bank] ([BankID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);
