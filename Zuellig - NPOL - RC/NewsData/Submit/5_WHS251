CREATE TABLE [dbo].[tblEmpShift] (
	[EmployeeID] nvarchar(25) NOT NULL, 
	[FromDate] datetime NOT NULL, 
	[FromDay] nvarchar(20), 
	[ToDate] datetime, 
	[ToDay] nvarchar(20), 
	[ShiftID] nvarchar(10), 
	[Notes] nvarchar(50), 
	[DepID] nvarchar(10), 
	[PosID] nvarchar(10),
	CONSTRAINT [PK_tblEmpShift] PRIMARY KEY ([EmployeeID], [FromDate]), 
	FOREIGN KEY ([ShiftID])
		REFERENCES [dbo].[tblShift] ([ShiftID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([EmployeeID])
		REFERENCES [dbo].[tblEmployee] ([EmployeeID])
		ON UPDATE CASCADE ON DELETE CASCADE
);


CREATE TABLE [dbo].[tblEmpShift_Detail] (
	[EmployeeID] nvarchar(25) NOT NULL, 
	[MonthID] int NOT NULL, 
	[YearID] int NOT NULL, 
