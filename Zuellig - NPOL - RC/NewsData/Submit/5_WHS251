CREATE TABLE [dbo].[tblRef_Nationality] (
	[NationalityID] nvarchar(10) NOT NULL PRIMARY KEY, 
	[NationalityName] nvarchar(100)
);


CREATE TABLE [dbo].[tblRef_Region] (
	[RegionID] nvarchar(10) NOT NULL PRIMARY KEY, 
	[RegionKey] nvarchar(1), 
	[RegionName] nvarchar(50), 
	[RegionParent] nvarchar(10), 
	[RegionTypeID] nvarchar(10), 
	[Notes] nvarchar(100)
);
CREATE TRIGGER [dbo].[tblRef_Region_UTrig] ON tblRef_Region 
FOR UPDATE AS
SET NOCOUNT ON
/* * CASCADE UPDATES TO 'tblEmployee', 'tblRef_Region' */

IF UPDATE(RegionID)
    BEGIN
--/// tblRef_Region --- RegionParent
       --// Update RegionID
       UPDATE tblRef_Region
       SET tblRef_Region.RegionParent = inserted.RegionID
       FROM tblRef_Region, deleted, inserted
       WHERE deleted.RegionID = tblRef_Region.RegionParent

--/// tblEmployee
       --// Update RegionID
       UPDATE tblEmployee
       SET tblEmployee.RegionID = inserted.RegionID
       FROM tblEmployee, deleted, inserted
       WHERE deleted.RegionID = tblEmployee.RegionID

	--// Update RegionID
       UPDATE tblEmployee
       SET tblEmployee.SubRegionID = inserted.RegionID
       FROM tblEmployee, deleted, inserted
       WHERE deleted.RegionID = tblEmployee.SubRegionID
    END

IF UPDATE(RegionName)
    BEGIN
--/// tblEmployee
       --// Update RegionName
       UPDATE tblEmployee
       SET tblEmployee.RegionName = inserted.RegionName
       FROM tblEmployee, deleted, inserted
       WHERE deleted.RegionName = tblEmployee.RegionName

       --// Update SubRegionName
       UPDATE tblEmployee
       SET tblEmployee.SubRegionName = inserted.RegionName
       FROM tblEmployee, deleted, inserted
       WHERE deleted.RegionName = tblEmployee.SubRegionName
    END;


CREATE TABLE [dbo].[tbl_TrainSupplier] (
	[SupplierID] nvarchar(20) NOT NULL PRIMARY KEY, 
	[RegionID] nvarchar(10), 
	[NationalityID] nvarchar(10), 
	[SupplierName] nvarchar(100), 
	[Website] nvarchar(50), 
	[Reduction] nvarchar(255), 
	[Amount] float, 
	[Address] nvarchar(255), 
	[Phone] nvarchar(50), 
	[Fax] nvarchar(50), 
	[Email] nvarchar(150), 
	[VATCode] nvarchar(50), 
	[MainFieldID] nvarchar(10), 
	[Notes] nvarchar(255), 
	FOREIGN KEY ([RegionID])
		REFERENCES [dbo].[tblRef_Region] ([RegionID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([NationalityID])
		REFERENCES [dbo].[tblRef_Nationality] ([NationalityID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);
CREATE UNIQUE NONCLUSTERED INDEX [IX_tbl_TrainSupplier]
	ON [dbo].[tbl_TrainSupplier] ([SupplierID], [RegionID]);
CREATE TRIGGER [dbo].[tbl_TrainSupplier_DTrig] ON dbo.tbl_TrainSupplier
FOR DELETE 
AS

/* * PREVENT DELETES IF DEPENDENT RECORDS IN 'tbl_TrainPlan' */
IF (SELECT COUNT(*) FROM deleted, tbl_TrainPlan WHERE (deleted.SupplierID = tbl_TrainPlan.SupplierID)) > 0
    BEGIN
        RAISERROR 44445 'You can not delete this data because you use it in Training plan !' --'The record can''t be deleted or changed. Since related records exist in table ''tblCurriculumVitae'', referential integrity rules would be violated.'
        ROLLBACK TRANSACTION
    END

/* * PREVENT DELETES IF DEPENDENT RECORDS IN 'tblTRain_Request' */

IF (SELECT COUNT(*) FROM deleted, tbl_TRainRequest WHERE (deleted.SupplierID = tbl_TRainRequest.SupplierID)) > 0
    BEGIN
        RAISERROR 44445 'You can not delete this data because you use it in Training request !' --'The record can''t be deleted or changed. Since related records exist in table ''tblCurriculumVitae'', referential integrity rules would be violated.'
        ROLLBACK TRANSACTION
    END;


CREATE TABLE [dbo].[tbl_TrainCourse] (
	[KeyID] int NOT NULL PRIMARY KEY, 
	[CourseID] nvarchar(50), 
	[CourseName] nvarchar(500), 
	[CourseName_Eng] nvarchar(500), 
	[SupplierID] nvarchar(20), 
	[FieldID] nvarchar(20), 
	[SkillID] nvarchar(20), 
	[NumEmp] int, 
	[StartTime] nvarchar(255), 
	[TotalHours] float, 
	[TrainPlace] nvarchar(500), 
	[Trainer] nvarchar(255), 
	[Target] nvarchar(500), 
	[TrainFee] float, 
	[DocumentFee] float, 
	[OtherFee] float, 
	[TotalFee] float, 
	[CurrType] nvarchar(20), 
	[Notes] nvarchar(500), 
	[LangName] nvarchar(100), 
	[DegreeName] nvarchar(100), 
	[IsKeepBonus] bit, 
	FOREIGN KEY ([SupplierID])
		REFERENCES [dbo].[tbl_TrainSupplier] ([SupplierID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Số lượng học viên',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainCourse',@level2type=N'Column',@level2name=N'NumEmp';


CREATE TABLE [dbo].[tbl_TrainCourse_Excel_Tmp] (
	[CourseID] nvarchar(50), 
	[CourseName] nvarchar(500), 
	[CourseName_Eng] nvarchar(500), 
	[SupplierID] nvarchar(20), 
	[FieldID] nvarchar(20), 
	[SkillID] nvarchar(20), 
	[NumEmp] int, 
	[StartTime] nvarchar(255), 
	[TotalHours] float, 
	[TrainPlace] nvarchar(500), 
	[Trainer] nvarchar(255), 
	[Target] nvarchar(500), 
	[TrainFee] float, 
	[DocumentFee] float, 
	[OtherFee] float, 
	[TotalFee] float, 
	[CurrType] nvarchar(20), 
	[Notes] nvarchar(500), 
	[IsDelete] bit, 
	[KeyID] int NOT NULL IDENTITY(12,1), 
	[LangName] nvarchar(100), 
	[DegreeName] nvarchar(100)
);
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Số lượng học viên',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainCourse_Excel_Tmp',@level2type=N'Column',@level2name=N'NumEmp';


CREATE TABLE [dbo].[tbl_TrainOrient] (
	[EmployeeID] nvarchar(25) NOT NULL, 
	[DateChange] datetime NOT NULL, 
	[TrainPlace] nvarchar(255), 
	[Notes] nvarchar(255), 
	[IsTrain] bit, 
	[SectionName] nvarchar(100), 
	[LineName] nvarchar(100), 
	[DepName] nvarchar(100), 
	[WorkingLocation] nvarchar(100), 
	[ControlLocation] nvarchar(100),
	CONSTRAINT [PK_tbl_TrainOrient] PRIMARY KEY ([EmployeeID], [DateChange]), 
	FOREIGN KEY ([EmployeeID])
		REFERENCES [dbo].[tblEmployee] ([EmployeeID])
		ON UPDATE CASCADE ON DELETE CASCADE
);


CREATE TABLE [dbo].[tbl_TrainOrient_Excel_Tmp] (
	[EmployeeID] nvarchar(25), 
	[DateChange] datetime, 
	[TrainPlace] nvarchar(255), 
	[Notes] nvarchar(255), 
	[IsDelete] bit, 
	[KeyID] int NOT NULL IDENTITY(1,1) PRIMARY KEY, 
	[IsTrain] bit, 
	[SectionName] nvarchar(100), 
	[LineName] nvarchar(100), 
	[DepName] nvarchar(100), 
	[WorkingLocation] nvarchar(100), 
	[ControlLocation] nvarchar(100)
);


CREATE TABLE [dbo].[tbl_TrainPlan] (
	[DateChange] datetime NOT NULL, 
	[CourseName] nvarchar(255) NOT NULL, 
	[CourseID] nvarchar(50), 
	[CourseName_Eng] nvarchar(500), 
	[Priority] int, 
	[TrainTime] nvarchar(50), 
	[NumEmp] int, 
	[FieldID] nvarchar(20), 
	[SkillID] nvarchar(20), 
	[TrainTypeID] nvarchar(10), 
	[SupplierID] nvarchar(50), 
	[TrainPlace] nvarchar(255), 
	[Trainer] nvarchar(255), 
	[ParticipantType] nvarchar(255), 
	[LangID] nvarchar(10), 
	[CompFee] float, 
	[EmpFee] float, 
	[TotalFee] float, 
	[CurrType] nvarchar(20), 
	[FromDate] datetime, 
	[ToDate] datetime, 
	[Reason] nvarchar(500), 
	[Achieve] nvarchar(500), 
	[ContactName] nvarchar(255), 
	[EmailTo] nvarchar(255), 
	[Notes] nvarchar(255), 
	[IsFromPDR] bit,
	CONSTRAINT [PK_tbl_TrainPlan] PRIMARY KEY ([DateChange], [CourseName])
);
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Không sử dụng(Lĩnh vực đào tạo)',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainPlan',@level2type=N'Column',@level2name=N'FieldID';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Nhà cung cấp',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainPlan',@level2type=N'Column',@level2name=N'SupplierID';
