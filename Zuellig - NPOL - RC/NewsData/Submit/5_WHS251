CREATE TABLE [dbo].[tblRecorder] (
	[Ngay_CC] datetime NOT NULL, 
	[EmployeeID] nvarchar(25) NOT NULL, 
	[SweepIn] datetime NOT NULL, 
	[SweepOut] datetime, 
	[SweepIn_Cut] datetime, 
	[SweepOut_Cut] datetime, 
	[ShiftID] nvarchar(10), 
	[Mark] bit, 
	[Addtime] int, 
	[FirstName] nvarchar(50), 
	[LastName] nvarchar(50), 
	[SectionID] nvarchar(10), 
	[LineID] nvarchar(10), 
	[DepID] nvarchar(10), 
	[TeamID] nvarchar(10), 
	[PartID] nvarchar(10), 
	[STT] int, 
	[IsHide] bit, 
	[IsNoScan] bit,
	CONSTRAINT [PK_tblRecorder] PRIMARY KEY ([Ngay_CC], [EmployeeID], [SweepIn]), 
	FOREIGN KEY ([EmployeeID])
		REFERENCES [dbo].[tblEmployee] ([EmployeeID])
		ON UPDATE CASCADE ON DELETE CASCADE, 
	FOREIGN KEY ([ShiftID])
		REFERENCES [dbo].[tblShift] ([ShiftID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);
CREATE NONCLUSTERED INDEX [IX_tblRecorder]
	ON [dbo].[tblRecorder] ([Ngay_CC], [EmployeeID], [SweepIn]);
CREATE NONCLUSTERED INDEX [IX_tblRecorder_Dep]
	ON [dbo].[tblRecorder] ([EmployeeID], [SectionID], [LineID]);


