CREATE TABLE [dbo].[tblRef_Position] (
	[PosID] nvarchar(10) NOT NULL PRIMARY KEY, 
	[PosName] nvarchar(100), 
	[WDInMonth] smallint, 
	[PosName_VN] nvarchar(100), 
	[PosName_Japan] nvarchar(100), 
	[DescriptionJob] text, 
	[DescriptionJob_Foreign] text, 
	[RankID] nvarchar(10), 
	[OrderNo] int, 
	[IsDiligence] bit, 
	[IsNotOT] bit, 
	[IsNotWD] bit, 
	[IsManage] bit, 
	[WHInDay] float, 
	[ColorID] nvarchar(10), 
	FOREIGN KEY ([RankID])
		REFERENCES [dbo].[tblRef_PositionRank] ([RankID])
		ON UPDATE NO ACTION ON DELETE NO ACTION
);
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Co xet chuyen can hay khong?',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tblRef_Position',@level2type=N'Column',@level2name=N'IsDiligence';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Co khong tinh tang ca hay khong?',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tblRef_Position',@level2type=N'Column',@level2name=N'IsNotOT';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Co xet tru ngay cong theo gioi han hay khong? ',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tblRef_Position',@level2type=N'Column',@level2name=N'IsNotWD';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'=1: Cấp quản lý (quyền trưởng ban trở lên)',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tblRef_Position',@level2type=N'Column',@level2name=N'IsManage';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Màu của đồng phục',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tblRef_Position',@level2type=N'Column',@level2name=N'ColorID';

CREATE TRIGGER [dbo].[tblRef_Position_UTrig] ON dbo.tblRef_Position
FOR UPDATE AS
SET NOCOUNT ON
/* * CASCADE UPDATES TO 'tblEmployee' */

IF UPDATE(PosID)
    BEGIN
--/// tblEmployee
       --// Update PosID
       UPDATE tblEmployee
       SET tblEmployee.PosID = inserted.PosID
       FROM tblEmployee, deleted, inserted
       WHERE deleted.PosID = tblEmployee.PosID
    END

IF UPDATE(PosName)
    BEGIN
--/// tblEmployee
       --// Update PositionName
       UPDATE tblEmployee
       SET tblEmployee.PosName = inserted.PosName
       FROM tblEmployee, deleted, inserted
       WHERE deleted.PosName = tblEmployee.PosName
    END;


CREATE TABLE [dbo].[tblRef_PositionDecision] (
	[TypeID] nvarchar(10) NOT NULL PRIMARY KEY, 
	[TypeName] nvarchar(50)
);


CREATE TABLE [dbo].[tblEmpPos] (
	[EmployeeID] nvarchar(25) NOT NULL, 
	[DateChange] datetime NOT NULL, 
	[TypeID] nvarchar(10), 
	[PosID] nvarchar(10), 
	[Notes] nvarchar(100), 
	[Job] nvarchar(4000), 
	[JobRight] nvarchar(1000), 
	[Reason] nvarchar(255), 
	[DecisionNo] nvarchar(20), 
	[Amount] float, 
	[From_MY] nvarchar(7), 
	[To_MY] nvarchar(7), 
	[RankID] nvarchar(10),
	CONSTRAINT [PK_tblEmpPos] PRIMARY KEY ([EmployeeID], [DateChange]), 
	FOREIGN KEY ([TypeID])
		REFERENCES [dbo].[tblRef_PositionDecision] ([TypeID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([PosID])
		REFERENCES [dbo].[tblRef_Position] ([PosID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([EmployeeID])
		REFERENCES [dbo].[tblEmployee] ([EmployeeID])
		ON UPDATE CASCADE ON DELETE CASCADE
);
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Bãi nhiệm/bổ nhiệm',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tblEmpPos',@level2type=N'Column',@level2name=N'TypeID';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Ly do',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tblEmpPos',@level2type=N'Column',@level2name=N'Reason';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'So quyet dinh',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tblEmpPos',@level2type=N'Column',@level2name=N'DecisionNo';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Từ tháng năm',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tblEmpPos',@level2type=N'Column',@level2name=N'From_MY';


