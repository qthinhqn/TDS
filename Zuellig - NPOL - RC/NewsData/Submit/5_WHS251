CREATE TABLE [dbo].[tblEmpContract_Liability_2012_04] (
	[OrderNo] nvarchar(10), 
	[EmployeeID] nvarchar(25) NOT NULL, 
	[DateChange] datetime NOT NULL, 
	[CandID] nvarchar(25), 
	[ContractID] nvarchar(50), 
	[OfferID] nvarchar(50), 
	[TempDate] datetime, 
	[Place_Issue] nvarchar(255), 
	[Place_Issue_E] nvarchar(255), 
