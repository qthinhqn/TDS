CREATE TABLE [dbo].[A_CandSup] (
	[STT] int NOT NULL IDENTITY(1,1) PRIMARY KEY, 
	[SupID] nvarchar(255), 
	[SupName] nvarchar(255), 
	[Address] nvarchar(255), 
	[Phone] nvarchar(255), 
	[FAX] nvarchar(255), 
	[EMAIL] nvarchar(255), 
	[WEBSITE] nvarchar(255), 
	[Nationality] nvarchar(255), 
	[ContactName] nvarchar(255), 
	[ContactPos] nvarchar(255), 
	[ContacPhone] nvarchar(255), 
	[NationalityID] nvarchar(50)
);


CREATE TABLE [dbo].[A_Emp] (
	[No] float NOT NULL PRIMARY KEY, 
	[ID] nvarchar(255), 
	[EmployeeName] nvarchar(255), 
	[FamilyName] nvarchar(255), 
	[GivenName] nvarchar(255), 
	[CostLocation] nvarchar(255), 
	[Division] nvarchar(255), 
	[Departments] nvarchar(255), 
	[ReportsTo] nvarchar(255), 
	[EmploymentStatus] nvarchar(255), 
	[EmploymentType] nvarchar(255), 
	[EmployeeType] nvarchar(255), 
	[WorkHours] nvarchar(255), 
	[PayrollGroup] nvarchar(255), 
	[JobTitle] nvarchar(255), 
	[Gender] nvarchar(255), 
	[Birthday] smalldatetime, 
	[Contract_SignDate] datetime, 
	[Contract_StartDate] datetime, 
	[Contract_EndDate] datetime, 
	[SIMI_StartDate] datetime, 
	[HireDate] datetime, 
	[StartDate] datetime, 
	[TempEndDate] datetime, 
	[ContractSalaryIs] nvarchar(255), 
	[Currency] nvarchar(255), 
	[Contract_Salary] float, 
	[Probation_Salary] float, 
	[SIGross_Salary] nvarchar(255), 
	[Birthplace] nvarchar(255), 
	[Nationality] nvarchar(255), 
	[Position Classification] nvarchar(255), 
	[Job Title1] nvarchar(255), 
	[Payment Method] nvarchar(255), 
	[Resignation Reason] nvarchar(255), 
	[SubmitDate] datetime, 
	[LeftDate] datetime, 
	[FinalPaymentDate] datetime, 
	[Resignation Notes] nvarchar(255), 
	[TaxNo] nvarchar(255), 
	[TaxTypePIT] nvarchar(255), 
	[Taxes Filed By Employee] bit NOT NULL, 
	[Temp_Address] nvarchar(255), 
	[Education] nvarchar(255), 
	[SINo] nvarchar(255), 
	[SI_StartDate] nvarchar(255), 
	[Hospital] nvarchar(255), 
	[Work Permit No#] nvarchar(255), 
	[Work Permit Length] nvarchar(255), 
	[ID_Number] nvarchar(255), 
	[ID_IssuedPlace] nvarchar(255), 
	[ID_IssuedDate] smalldatetime, 
	[ID_ExpirationDate] nvarchar(255), 
	[Passport_Number] nvarchar(255), 
	[Passport_IssuedPlace] nvarchar(255), 
	[Passport_IssuedDate] nvarchar(255), 
	[Passport_ExpirationDate] nvarchar(255), 
	[AccountName] nvarchar(255), 
	[AccountNumber] nvarchar(255), 
	[Currency1] nvarchar(255), 
	[AccountCode] nvarchar(255), 
	[AccountRouting] nvarchar(255), 
	[Bank] nvarchar(255), 
	[Address] nvarchar(255), 
	[Address1] nvarchar(255), 
	[City] nvarchar(255), 
	[Ward] nvarchar(255), 
	[District] nvarchar(255), 
	[Province] nvarchar(255), 
	[Country] nvarchar(255), 
	[Zip/Postal] nvarchar(255), 
	[Phone] nvarchar(255), 
	[Mobile] nvarchar(255), 
	[Email] nvarchar(255), 
	[SecID] nvarchar(50), 
	[LineID] nvarchar(50), 
	[GroupID] nvarchar(50), 
	[ContractID] nvarchar(50), 
	[ContractTypeID] nvarchar(50), 
	[LeftTypeID] nvarchar(10), 
	[SexID] nvarchar(10), 
	[EducationID] nvarchar(10), 
	[PosID] nvarchar(10), 
	[CostLocationID] nvarchar(10), 
	[EmpTypeID] nvarchar(10), 
	[NationalityID] nvarchar(10), 
	[RaceID] nvarchar(10), 
	[HIPlaceID] nvarchar(10), 
	[BankID] nvarchar(10), 
	[cityID] nvarchar(10), 
	[districtID] nvarchar(10)
);
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'[Resign Notify Date]',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'A_Emp',@level2type=N'Column',@level2name=N'SubmitDate';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'[Last Work Date]',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'A_Emp',@level2type=N'Column',@level2name=N'LeftDate';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'[Last Pay Date]',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'A_Emp',@level2type=N'Column',@level2name=N'FinalPaymentDate';


