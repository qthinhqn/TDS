CREATE TABLE [dbo].[tbl_TrainRequest_Emp] (
	[RequestID] nvarchar(50) NOT NULL, 
	[EmployeeID] nvarchar(15) NOT NULL, 
	[Result] nvarchar(255), 
	[AgreementCode] nvarchar(50), 
	[AgreementDate] datetime, 
	[DegreeDate] datetime, 
	[Feedback] nvarchar(1000), 
	[Notes] nvarchar(255), 
	[Attendance] float, 
	[MidTest] float, 
	[FinalTest] float, 
	[Achieve] nvarchar(1000), 
	[Strength] nvarchar(1000), 
	[Weakness] nvarchar(1000), 
	[Recommend] nvarchar(1000),
	CONSTRAINT [PK_tbl_TrainRequest_Emp] PRIMARY KEY ([RequestID], [EmployeeID]), 
	FOREIGN KEY ([RequestID])
		REFERENCES [dbo].[tbl_TrainRequest] ([RequestID])
		ON UPDATE CASCADE ON DELETE CASCADE
);
