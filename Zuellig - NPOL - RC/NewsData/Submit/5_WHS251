	[D3] nvarchar(10), 
	[D4] nvarchar(10), 
	[D5] nvarchar(10), 
	[D6] nvarchar(10), 
	[D7] nvarchar(10), 
	[D8] nvarchar(10), 
	[D9] nvarchar(10), 
	[D10] nvarchar(10), 
	[D11] nvarchar(10), 
	[D12] nvarchar(10), 
	[D13] nvarchar(10), 
