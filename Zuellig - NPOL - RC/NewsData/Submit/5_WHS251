EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'thời gian học Đến ngày',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainPlan',@level2type=N'Column',@level2name=N'ToDate';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Lý do phát sinh nhu cầu đào tạo',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainPlan',@level2type=N'Column',@level2name=N'Reason';
