	[KeyID] int NOT NULL PRIMARY KEY, 
	[YearID] int NOT NULL, 
	[Serial] int, 
	[FY] nvarchar(10), 
	[FieldID] nvarchar(20), 
	[SkillID] nvarchar(20), 
	[Amount] float, 
	[CurrType] nvarchar(20), 
	[NumEmp] int, 
	[Notes] nvarchar(500), 
	[TrainTypeID] nvarchar(10)
);
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Năm tài chính',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainBudget_HR',@level2type=N'Column',@level2name=N'FY';


