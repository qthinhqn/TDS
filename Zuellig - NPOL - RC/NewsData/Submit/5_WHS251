CREATE TABLE [dbo].[tbl_TrainRef_Fee] (
	[TypeID] nvarchar(10) NOT NULL PRIMARY KEY, 
	[TypeName] nvarchar(50)
);


CREATE TABLE [dbo].[tbl_TrainRef_Field] (
	[FieldID] nvarchar(20) NOT NULL PRIMARY KEY, 
	[FieldName] nvarchar(100)
);
CREATE TRIGGER [dbo].[tbl_TrainRef_Field_DTrig] ON tbl_TrainRef_Field
FOR DELETE 
AS

IF (SELECT COUNT(*) FROM deleted, tbl_TrainSupplier WHERE (deleted.FieldID = tbl_TrainSupplier.MainFieldID)) > 0
    BEGIN
        RAISERROR 44445 'You can not delete this data because you use it in training''s field of supplier!' 
        ROLLBACK TRANSACTION
    END

IF (SELECT COUNT(*) FROM deleted, tbl_TrainRequest WHERE (deleted.FieldID = tbl_TrainRequest.FieldID)) > 0
    BEGIN
        RAISERROR 44445 'You can not delete this data because you use it in training''s employee!' 
        ROLLBACK TRANSACTION
    END

IF (SELECT COUNT(*) FROM deleted, tbl_TrainCourse WHERE (deleted.FieldID = tbl_TrainCourse.FieldID)) > 0
    BEGIN
        RAISERROR 44445 'You can not delete this data because you use it in training''s field of training course!' 
        ROLLBACK TRANSACTION
    END;
CREATE TRIGGER [dbo].[tbl_TrainRef_Field_UTrig] ON tbl_TrainRef_Field FOR UPDATE AS
SET NOCOUNT ON
/* * CASCADE UPDATES TO 'tbl_TrainSupplier_Field' */
IF UPDATE(FieldID)
       UPDATE tbl_TrainSupplier
       SET tbl_TrainSupplier.MainFieldID = inserted.FieldID
       FROM tbl_TrainSupplier, deleted, inserted
       WHERE deleted.FieldID = tbl_TrainSupplier.MainFieldID

       UPDATE tbl_TrainCourse
       SET tbl_TrainCourse.FieldID = inserted.FieldID
       FROM tbl_TrainCourse, deleted, inserted
       WHERE deleted.FieldID = tbl_TrainCourse.FieldID
	
       UPDATE tbl_TrainRequest
       SET tbl_TrainRequest.FieldID = inserted.FieldID
       FROM tbl_TrainRequest, deleted, inserted
       WHERE deleted.FieldID = tbl_TrainRequest.FieldID
	
       UPDATE tbl_TrainPlan
       SET tbl_TrainPlan.FieldID = inserted.FieldID
       FROM tbl_TrainPlan, deleted, inserted
       WHERE deleted.FieldID = tbl_TrainPlan.FieldID
	
       UPDATE tbl_TrainPropose
       SET tbl_TrainPropose.FieldID = inserted.FieldID
       FROM tbl_TrainPropose, deleted, inserted
       WHERE deleted.FieldID = tbl_TrainPropose.FieldID;


CREATE TABLE [dbo].[tbl_TrainRef_TrainType] (
	[TrainTypeID] nvarchar(10) NOT NULL PRIMARY KEY, 
	[TrainTypeName] nvarchar(100)
);
CREATE TRIGGER [dbo].[tbl_TrainRef_TrainType_UTrig] ON tbl_TrainRef_TrainType FOR UPDATE AS
SET NOCOUNT ON

/* * CASCADE UPDATES TO 'tbl_TrainSupplier_Field' */
IF UPDATE(TrainTypeID)      
	
       UPDATE tbl_TrainRequest
       SET tbl_TrainRequest.TrainTypeID = inserted.TrainTypeID
       FROM tbl_TrainRequest, deleted, inserted
       WHERE deleted.TrainTypeID = tbl_TrainRequest.TrainTypeID
	
       UPDATE tbl_TrainPlan
       SET tbl_TrainPlan.TrainTypeID = inserted.TrainTypeID
       FROM tbl_TrainPlan, deleted, inserted
       WHERE deleted.TrainTypeID = tbl_TrainPlan.TrainTypeID
	
       UPDATE tbl_TrainPropose
       SET tbl_TrainPropose.TrainTypeID = inserted.TrainTypeID
       FROM tbl_TrainPropose, deleted, inserted
       WHERE deleted.TrainTypeID = tbl_TrainPropose.TrainTypeID;


CREATE TABLE [dbo].[tbl_TrainRequest] (
	[RequestID] nvarchar(50) NOT NULL PRIMARY KEY, 
	[DateChange] datetime, 
	[CourseID] nvarchar(50), 
	[CourseName] nvarchar(255), 
	[CourseName_Eng] nvarchar(255), 
	[SkillID] nvarchar(20), 
	[FieldID] nvarchar(20), 
	[TrainTypeID] nvarchar(10), 
	[SupplierID] nvarchar(20), 
	[FromDate] datetime, 
	[ToDate] datetime, 
	[TotalTime] float, 
	[JoinRate] float, 
	[LangID] nvarchar(10), 
	[TrainPlace] nvarchar(255), 
	[Trainer] nvarchar(500), 
	[Target] nvarchar(500), 
	[TotalFee] float, 
	[CurrType] nvarchar(20), 
	[CompFee] float, 
	[EmpFee] float, 
	[Notes] nvarchar(255), 
	[IsCancel] bit, 
	[IsFromPDR] bit, 
	[IsAgreement] bit, 
	[StartDate] datetime, 
	[EndDate] datetime, 
	FOREIGN KEY ([SupplierID])
		REFERENCES [dbo].[tbl_TrainSupplier] ([SupplierID])
		ON UPDATE CASCADE ON DELETE NO ACTION, 
	FOREIGN KEY ([TrainTypeID])
		REFERENCES [dbo].[tbl_TrainRef_TrainType] ([TrainTypeID])
		ON UPDATE CASCADE ON DELETE NO ACTION
);
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Kỹ năng',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainRequest',@level2type=N'Column',@level2name=N'SkillID';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Lĩnh vực đào tạo/ Chuyên môn đào tạo',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainRequest',@level2type=N'Column',@level2name=N'FieldID';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Loại hình (Internal/External)',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainRequest',@level2type=N'Column',@level2name=N'TrainTypeID';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Nhà cung cấp',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainRequest',@level2type=N'Column',@level2name=N'SupplierID';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Tỷ lệ % tham gia khoá học',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainRequest',@level2type=N'Column',@level2name=N'JoinRate';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Ngôn ngữ',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainRequest',@level2type=N'Column',@level2name=N'LangID';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Nơi học',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainRequest',@level2type=N'Column',@level2name=N'TrainPlace';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Người hướng dẫn',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainRequest',@level2type=N'Column',@level2name=N'Trainer';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Có huỷ hay không?',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainRequest',@level2type=N'Column',@level2name=N'IsCancel';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Lấy thông tin từ đánh giá??',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainRequest',@level2type=N'Column',@level2name=N'IsFromPDR';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Có cam kết tài chính hay không?',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainRequest',@level2type=N'Column',@level2name=N'IsAgreement';
EXEC sp_addextendedproperty @name=N'MS_Description',@value=N'Ngày bắt đầu cam kết',@level0type=N'USER',@level0name=N'dbo',@level1type=N'Table',@level1name=N'tbl_TrainRequest',@level2type=N'Column',@level2name=N'StartDate';
