	[OTDay_After] numeric(38, 1), 
	[OTNight_Before] numeric(38, 1), 
	[OTNight_After] numeric(38, 1), 
	[OTD_Before] numeric(38, 1), 
	[OTD_After] numeric(38, 1), 
	[OTN_Before] numeric(38, 1), 
	[OTN_After] numeric(38, 1)
);


